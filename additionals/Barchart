foreach xfitem [array names xfbarchartpsOptions] {
	lappend xfbarchartpsDefaults $xfitem
	lappend xfbarchartpsDefaults [string trim $xfbarchartpsOptions($xfitem)]
      }
eval "$xfbarchart_path postscript configure $xfbarchartpsDefaults"
xfbarchartdestroy.xfbarchartps}  -padx {11}  -pady {4}  -text {OK}

  # build widget .xfbarchartps.frame15.button10
