  radiobutton .xfbarchartelem.frame7.frame21.radiobutton2  -highlightthickness {0}  -pady {0}  -text {Sunken}  -value {sunken}  -variable {xfbarchartelemOptions(-relief)}  -width {7} -anchor w
  radiobutton .xfbarchartelem.frame7.frame21.radiobutton3  -highlightthickness {0}  -pady {0}  -text {Flat}  -value {flat}  -variable {xfbarchartelemOptions(-relief)}  -width {7} -anchor w
  pack  .xfbarchartelem.frame7.frame21 -fill x
  pack  .xfbarchartelem.frame7.frame21.label -side left
  pack  .xfbarchartelem.frame7.frame21.radiobutton1 -side left
  pack  .xfbarchartelem.frame7.frame21.radiobutton2 -side left
