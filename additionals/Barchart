  radiobutton .xfbarchartelem.frame7.frame48.radiobutton1 -text {Yes} -value {1} -variable {xfbarchartelemOptions(-hide)}
  radiobutton .xfbarchartelem.frame7.frame48.radiobutton2 -text {No}  -value {0} -variable {xfbarchartelemOptions(-hide)}
  pack  .xfbarchartelem.frame7.frame48 -fill x
  pack  .xfbarchartelem.frame7.frame48.label -side left
  pack  .xfbarchartelem.frame7.frame48.radiobutton1 -side left
  pack  .xfbarchartelem.frame7.frame48.radiobutton2 -side left

