# procedure to show window xfbarchartshow.xfbarchartconf
proc xfbarchartshow.xfbarchartconf { args} {
  global xfBind

  # build widget .xfbarchartconf
  catch "destroy .xfbarchartconf"
  toplevel .xfbarchartconf 

  # Window manager configurations

  wm positionfrom .xfbarchartconf ""
  wm sizefrom .xfbarchartconf ""
  wm maxsize .xfbarchartconf 1156 849
  wm minsize .xfbarchartconf 104 1
  wm title .xfbarchartconf {Barchart Configure Tool}

  # build widget .xfbarchartconf.cbutton_General
  checkbutton .xfbarchartconf.cbutton_General  -command {ConfigureGeneral}  -text {General}  -variable {varGeneral}

  # build widget .xfbarchartconf.cbutton_Element
  checkbutton .xfbarchartconf.cbutton_Element  -command {ConfigureElement}  -text {Element}  -variable {varElement}

  # build widget .xfbarchartconf.cbutton_Axis
  checkbutton .xfbarchartconf.cbutton_Axis  -command {ConfigureAxis}  -text {Axis}  -variable {varAxis}

  # build widget .xfbarchartconf.cbutton_Legend
  checkbutton .xfbarchartconf.cbutton_Legend  -command {ConfigureLegend}  -text {Legend}  -variable {varLegend}

  # build widget .xfbarchartconf.cbutton_Crosshairs
  checkbutton .xfbarchartconf.cbutton_Crosshairs  -command {ConfigureCrosshairs}  -text {Crosshairs}  -variable {varCrosshairs}

  # build widget .xfbarchartconf.cbutton_PostScript
  checkbutton .xfbarchartconf.cbutton_PostScript  -command {ConfigurePostScript}  -text {PostScript}  -variable {varPostScript}

  # build widget .xfbarchartconf.quit
  button .xfbarchartconf.quit  -command {xfbarchartdestroy.xfbarchartconf}  -padx {11}  -pady {4}  -text {quit}

  # build widget .xfbarchartconf.frame0
  frame .xfbarchartconf.frame0  -borderwidth {2}  -height {30}  -relief {ridge}  -width {30}

  # build widget .xfbarchartconf.frame0.label1
  label .xfbarchartconf.frame0.label1  -background {steelblue}  -font {helvetica 12 bold}  -foreground {white}  -relief {groove}  -text {Path:}

  # build widget .xfbarchartconf.frame0.entry2
  entry .xfbarchartconf.frame0.entry2  -background {lightblue}  -font {helvetica 12 bold}

  # pack master .xfbarchartconf.frame0
  pack configure .xfbarchartconf.frame0.label1  -fill y  -side left
  pack configure .xfbarchartconf.frame0.entry2  -expand 1  -fill x  -side left

  # pack master .xfbarchartconf
  pack configure .xfbarchartconf.frame0  -fill x
  pack configure .xfbarchartconf.cbutton_General  -anchor w
  pack configure .xfbarchartconf.cbutton_Element  -anchor w
  pack configure .xfbarchartconf.cbutton_Axis  -anchor w
  pack configure .xfbarchartconf.cbutton_Legend  -anchor w
  pack configure .xfbarchartconf.cbutton_Crosshairs  -anchor w
  pack configure .xfbarchartconf.cbutton_PostScript  -anchor w
  pack configure .xfbarchartconf.quit

  .xfbarchartconf.frame0.entry2 insert end {.frame0.xfbarchart0}
}

proc xfbarchartdestroy.xfbarchartconf {} {# xf ignore me 7
    catch "destroy .xfbarchartconf"
}


# procedure to show window xfbarchartshow.xfbarchartelem
proc xfbarchartshow.xfbarchartelem { args} {
  global xfBind

  # build widget .xfbarchartelem
  catch "destroy .xfbarchartelem"
  toplevel .xfbarchartelem   -relief {raised}

  # Window manager configurations
  wm positionfrom .xfbarchartelem ""
  wm sizefrom .xfbarchartelem ""
  wm maxsize .xfbarchartelem 1152 849
  wm minsize .xfbarchartelem 104 1
  wm title .xfbarchartelem {Barchart Element Configuration Options}

  frame .xfbarchartelem.frame22  -borderwidth {2}  -height {30}  -relief {raised}  -width {30}
  label .xfbarchartelem.frame22.label21  -borderwidth {0}  -text {Widget path:}
  label .xfbarchartelem.frame22.label25  -borderwidth {0}  -text {.frame0}  -textvariable {xfbarchart_parent}
  pack configure .xfbarchartelem.frame22 -fill x

  frame .xfbarchartelem.frame7 -borderwidth {2}  -height {30}  -relief {raised}  -width {30}
  pack  .xfbarchartelem.frame7 -fill x

  frame .xfbarchartelem.frame7.frame22  -height {30}  -width {30}
  label .xfbarchartelem.frame7.frame22.label1  -borderwidth {0}  -text {Name:}  -width {16} -anchor e
  label .xfbarchartelem.frame7.frame22.label2  -borderwidth {0}  -text {barchart0}  -textvariable {xfbarchart_name}
  pack  .xfbarchartelem.frame7.frame22  -fill x
  pack  .xfbarchartelem.frame7.frame22.label1 -side left
  pack  .xfbarchartelem.frame7.frame22.label2 -anchor w -pady 4

  frame .xfbarchartelem.frame7.elemnames
  label .xfbarchartelem.frame7.elemnames.label  -borderwidth {0}  -text {Elements:}  -width {16} -anchor e
  pack  .xfbarchartelem.frame7.elemnames -fill x
  pack  .xfbarchartelem.frame7.elemnames.label -side left

  frame .xfbarchartelem.frame7.elemnames.frame
  pack  .xfbarchartelem.frame7.elemnames.frame  -fill x

  frame .xfbarchartelem.frame7.frame26  -height {30}  -width {30}
  label .xfbarchartelem.frame7.frame26.label18  -borderwidth {0}  -text {Activepen:}  -width {16} -anchor e
  entry .xfbarchartelem.frame7.frame26.entry19  -textvariable {xfbarchartelemOptions(-activepen)}
  pack  .xfbarchartelem.frame7.frame26  -fill x
  pack  .xfbarchartelem.frame7.frame26.label18 -side left
  pack  .xfbarchartelem.frame7.frame26.entry19 -expand 1 -fill x  -side left

  frame .xfbarchartelem.frame7.frame100 -height 30 -width 30
  label .xfbarchartelem.frame7.frame100.label -borderwidth 0 -text {Background:} -width 16 -anchor e
  entry .xfbarchartelem.frame7.frame100.entry -textvariable {xfbarchartelemOptions(-background)}
  bind  .xfbarchartelem.frame7.frame100.entry $xfBind(configure) {set xfbarchartnewval [tk_chooseColor -parent .xfbarchartelem]
  if {$xfbarchartnewval != ""} {set xfbarchartelemOptions(-background) $xfbarchartnewval}}
  pack  .xfbarchartelem.frame7.frame100 -fill x
  pack  .xfbarchartelem.frame7.frame100.label -side left
  pack  .xfbarchartelem.frame7.frame100.entry -expand 1 -fill x -side left

  frame .xfbarchartelem.frame7.frame29  -height {30}  -width {30}
  label .xfbarchartelem.frame7.frame29.label  -borderwidth {0}  -text {Barwidth:}  -width {16} -anchor e
  entry .xfbarchartelem.frame7.frame29.entry  -textvariable {xfbarchartelemOptions(-barwidth)}
  pack  .xfbarchartelem.frame7.frame29 -fill x
  pack  .xfbarchartelem.frame7.frame29.label -side left
  pack  .xfbarchartelem.frame7.frame29.entry -expand 1 -fill x -side left

  frame .xfbarchartelem.frame7.frame30  -height {30}  -width {30}
  label .xfbarchartelem.frame7.frame30.label  -borderwidth {0}  -text {Borderwidth:}  -width {16} -anchor e
  entry .xfbarchartelem.frame7.frame30.entry  -textvariable {xfbarchartelemOptions(-borderwidth)}
  pack  .xfbarchartelem.frame7.frame30 -fill x
  pack  .xfbarchartelem.frame7.frame30.label -side left
  pack  .xfbarchartelem.frame7.frame30.entry -expand 1 -fill x -side left

  frame .xfbarchartelem.frame7.frame31 -height {30}  -width {30}
  label .xfbarchartelem.frame7.frame31.label  -borderwidth {0}  -text {Foreground:}  -width {16} -anchor e
  entry .xfbarchartelem.frame7.frame31.entry  -textvariable {xfbarchartelemOptions(-foreground)}
  bind  .xfbarchartelem.frame7.frame31.entry $xfBind(configure) {set xfbarchartnewval [tk_chooseColor -parent .xfbarchartelem]
  if {$xfbarchartnewval != ""} {set xfbarchartelemOptions(-foreground) $xfbarchartnewval}}
  pack  .xfbarchartelem.frame7.frame31 -fill x
  pack  .xfbarchartelem.frame7.frame31.label -side left
  pack  .xfbarchartelem.frame7.frame31.entry -expand 1 -fill x -side left

