  # build widget .xfbarchartaxis.frame7.frame26.label18
  label .xfbarchartaxis.frame7.frame26.label18  -borderwidth {0}  -text {Titlefont:}  -width {16} -anchor e

  # build widget .xfbarchartaxis.frame7.frame26.entry19
