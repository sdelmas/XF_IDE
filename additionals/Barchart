  pack configure .xfbarchartps.frame8.button12  -expand 1  -fill x  -side left

  # pack master .xfbarchartps.frame15
  pack configure .xfbarchartps.frame15.button9  -expand 1  -fill x  -side left
  pack configure .xfbarchartps.frame15.button10  -expand 1  -fill x  -side left
  pack configure .xfbarchartps.frame15.checkbutton16  -expand 1  -fill both  -side left
  pack configure .xfbarchartps.frame15.button12  -expand 1  -fill x  -side left

  # pack master .xfbarchartps.frame22
  pack configure .xfbarchartps.frame22.label21  -anchor e  -expand 1  -side left
  pack configure .xfbarchartps.frame22.label25  -anchor w  -expand 1  -side left

  # pack master .xfbarchartps
  pack configure .xfbarchartps.frame22  -fill x
  pack configure .xfbarchartps.frame7  -expand 1  -fill both
  pack configure .xfbarchartps.frame8  -fill x
  pack configure .xfbarchartps.frame15  -fill x
}

proc xfbarchartdestroy.xfbarchartps {} {
     catch "destroy .xfbarchartps"
}


# procedure to show window xfbarchartshow.xfbarchartxhair
proc xfbarchartshow.xfbarchartxhair { args} {
  global xfBind

  # build widget .xfbarchartxhair
  catch "destroy .xfbarchartxhair"
  toplevel .xfbarchartxhair   -relief {raised}

  # Window manager configurations
  wm positionfrom .xfbarchartxhair ""
  wm sizefrom .xfbarchartxhair ""
  wm maxsize .xfbarchartxhair 1152 849
  wm minsize .xfbarchartxhair 104 10
  wm title .xfbarchartxhair {Barchart Crosshairs Configuration}

  # build widget .xfbarchartxhair.frame7
  frame .xfbarchartxhair.frame7  -borderwidth {2}  -height {30}  -relief {raised}  -width {30}

  # build widget .xfbarchartxhair.frame7.frame17
  frame .xfbarchartxhair.frame7.frame17  -height {30}  -width {30}

  # build widget .xfbarchartxhair.frame7.frame17.label18
  label .xfbarchartxhair.frame7.frame17.label18  -borderwidth {0}  -text {Color:}  -width {16} -anchor e

  # build widget .xfbarchartxhair.frame7.frame17.entry19
  entry .xfbarchartxhair.frame7.frame17.entry19  -textvariable {xfbarchartxhairOptions(-color)}
  # bindings
  bind .xfbarchartxhair.frame7.frame17.entry19 $xfBind(configure) {set xfbarchartnewval [tk_chooseColor -parent .xfbarchartxhair]
if {$xfbarchartnewval != ""} {set xfbarchartxhairOptions(-color) $xfbarchartnewval}}

  # build widget .xfbarchartxhair.frame7.frame22
  frame .xfbarchartxhair.frame7.frame22  -height {30}  -width {30}

  # build widget .xfbarchartxhair.frame7.frame22.label21
  label .xfbarchartxhair.frame7.frame22.label21  -borderwidth {0}  -text {Name:}  -width {16} -anchor e

  # build widget .xfbarchartxhair.frame7.frame22.label25
  label .xfbarchartxhair.frame7.frame22.label25  -borderwidth {0}  -text {barchart0}  -textvariable {xfbarchart_name}

  # build widget .xfbarchartxhair.frame7.frame26
  frame .xfbarchartxhair.frame7.frame26  -height {30}  -width {30}

  # build widget .xfbarchartxhair.frame7.frame26.label18
  label .xfbarchartxhair.frame7.frame26.label18  -borderwidth {0}  -text {Dashes:}  -width {16} -anchor e

  # build widget .xfbarchartxhair.frame7.frame26.entry19
  entry .xfbarchartxhair.frame7.frame26.entry19  -textvariable {xfbarchartxhairOptions(-dashes)}

  # build widget .xfbarchartxhair.frame7.frame27
  frame .xfbarchartxhair.frame7.frame27  -height {30}  -width {30}

  # build widget .xfbarchartxhair.frame7.frame27.label18
  label .xfbarchartxhair.frame7.frame27.label18  -borderwidth {0}  -text {Linewidth:}  -width {16} -anchor e

  # build widget .xfbarchartxhair.frame7.frame27.entry19
  entry .xfbarchartxhair.frame7.frame27.entry19  -textvariable {xfbarchartxhairOptions(-linewidth)}

  # build widget .xfbarchartxhair.frame7.frame28
  frame .xfbarchartxhair.frame7.frame28  -height {30}  -width {30}

  # build widget .xfbarchartxhair.frame7.frame28.label18
  label .xfbarchartxhair.frame7.frame28.label18  -borderwidth {0}  -text {Position:}  -width {16} -anchor e

  # build widget .xfbarchartxhair.frame7.frame28.entry19
  entry .xfbarchartxhair.frame7.frame28.entry19  -textvariable {xfbarchartxhairOptions(-position)}

  # build widget .xfbarchartxhair.frame7.frame40
  frame .xfbarchartxhair.frame7.frame40  -height {30}  -width {30}

  # build widget .xfbarchartxhair.frame7.frame40.label18
  label .xfbarchartxhair.frame7.frame40.label18  -borderwidth {0}  -text {Hide:}  -width {16}

  # build widget .xfbarchartxhair.frame7.frame40.radiobutton41
  radiobutton .xfbarchartxhair.frame7.frame40.radiobutton41  -text {true}  -value {1}  -variable {xfbarchartxhairOptions(-hide)}

  # build widget .xfbarchartxhair.frame7.frame40.radiobutton42
  radiobutton .xfbarchartxhair.frame7.frame40.radiobutton42  -text {false}  -value {0}  -variable {xfbarchartxhairOptions(-hide)}

  # build widget .xfbarchartxhair.frame8
  frame .xfbarchartxhair.frame8  -height {30}  -width {30}

  # build widget .xfbarchartxhair.frame8.button9
  button .xfbarchartxhair.frame8.button9  -command {xfbarchartcfg_elem $xfbarchart_path}  -padx {11}  -pady {4}  -text {Element}

  # build widget .xfbarchartxhair.frame8.button10
  button .xfbarchartxhair.frame8.button10  -command {xfbarchartcfg_axis $xfbarchart_path}  -padx {11}  -pady {4}  -text {Axis}

  # build widget .xfbarchartxhair.frame8.button11
  button .xfbarchartxhair.frame8.button11  -command {xfbarchartcfg_legend $xfbarchart_path}  -padx {11}  -pady {4}  -text {Legend}

  # build widget .xfbarchartxhair.frame8.button12
  button .xfbarchartxhair.frame8.button12  -command {xfbarchartcfg_gen $xfbarchart_path}  -padx {11}  -pady {4}  -text {Parameters}

  # build widget .xfbarchartxhair.frame8.button13
  button .xfbarchartxhair.frame8.button13  -command {xfbarchartcfg_ps $xfbarchart_path}  -padx {11}  -pady {4}  -text {Postscript}

  # build widget .xfbarchartxhair.frame15
  frame .xfbarchartxhair.frame15  -height {30}  -width {30}

  # build widget .xfbarchartxhair.frame15.button9
  button .xfbarchartxhair.frame15.button9  -command {set xfbarchartDefaults ""
foreach xfitem [array names xfbarchartxhairOptions] {
	lappend xfbarchartxhairDefaults $xfitem
	lappend xfbarchartxhairDefaults [string trim $xfbarchartxhairOptions($xfitem)]
      }
eval "$xfbarchart_path crosshairs configure $xfbarchartxhairDefaults"
xfbarchartdestroy.xfbarchartxhair}  -padx {11}  -pady {4}  -text {OK}

  # build widget .xfbarchartxhair.frame15.button10
  button .xfbarchartxhair.frame15.button10  -command {set xfnewval "$xfbarchart_path crosshairs configure"
foreach xfitem [array names xfbarchartxhairOptions] {
	lappend xfnewval $xfitem
	lappend xfnewval [string trim $xfbarchartxhairOptions($xfitem)]
      }
eval $xfnewval}  -padx {11}  -pady {4}  -text {Apply}

  # build widget .xfbarchartxhair.frame15.button12
  button .xfbarchartxhair.frame15.button12  -command {eval "$xfbarchart_path crosshairs configure $xfbarchartxhairDefaults"
xfbarchartdestroy.xfbarchartxhair}  -padx {11}  -pady {4}  -text {Cancel}

  # build widget .xfbarchartxhair.frame15.checkbutton16
  checkbutton .xfbarchartxhair.frame15.checkbutton16  -text {Apply permanently}  -variable {xfConf(applyParameters)}

  # build widget .xfbarchartxhair.frame22
  frame .xfbarchartxhair.frame22  -borderwidth {2}  -height {30}  -relief {raised}  -width {30}

  # build widget .xfbarchartxhair.frame22.label21
  label .xfbarchartxhair.frame22.label21  -borderwidth {0}  -text {Widget path:}

  # build widget .xfbarchartxhair.frame22.label25
  label .xfbarchartxhair.frame22.label25  -borderwidth {0}  -text {.frame0}  -textvariable {xfbarchart_parent}

  # pack master .xfbarchartxhair.frame7
  pack configure .xfbarchartxhair.frame7.frame22  -fill x
  pack configure .xfbarchartxhair.frame7.frame17  -fill x
  pack configure .xfbarchartxhair.frame7.frame26  -fill x
  pack configure .xfbarchartxhair.frame7.frame27  -fill x
  pack configure .xfbarchartxhair.frame7.frame40  -fill x
  pack configure .xfbarchartxhair.frame7.frame28  -fill x

  # pack master .xfbarchartxhair.frame7.frame17
  pack configure .xfbarchartxhair.frame7.frame17.label18  -side left
  pack configure .xfbarchartxhair.frame7.frame17.entry19  -expand 1  -fill x  -side left

  # pack master .xfbarchartxhair.frame7.frame22
  pack configure .xfbarchartxhair.frame7.frame22.label21  -side left
  pack configure .xfbarchartxhair.frame7.frame22.label25  -anchor w  -pady 4

  # pack master .xfbarchartxhair.frame7.frame26
  pack configure .xfbarchartxhair.frame7.frame26.label18  -side left
  pack configure .xfbarchartxhair.frame7.frame26.entry19  -expand 1  -fill x  -side left

  # pack master .xfbarchartxhair.frame7.frame27
  pack configure .xfbarchartxhair.frame7.frame27.label18  -side left
  pack configure .xfbarchartxhair.frame7.frame27.entry19  -expand 1  -fill x  -side left

  # pack master .xfbarchartxhair.frame7.frame28
  pack configure .xfbarchartxhair.frame7.frame28.label18  -side left
  pack configure .xfbarchartxhair.frame7.frame28.entry19  -expand 1  -fill x  -side left

  # pack master .xfbarchartxhair.frame7.frame40
  pack configure .xfbarchartxhair.frame7.frame40.label18  -side left
  pack configure .xfbarchartxhair.frame7.frame40.radiobutton41  -side left
  pack configure .xfbarchartxhair.frame7.frame40.radiobutton42  -side left

  # pack master .xfbarchartxhair.frame8
  pack configure .xfbarchartxhair.frame8.button12  -expand 1  -fill x  -side left
  pack configure .xfbarchartxhair.frame8.button9  -expand 1  -fill x  -side left
  pack configure .xfbarchartxhair.frame8.button10  -expand 1  -fill x  -side left
  pack configure .xfbarchartxhair.frame8.button11  -expand 1  -fill x  -side left
