  # pack master .xfbarchartxhair.frame15
  pack configure .xfbarchartxhair.frame15.button9  -expand 1  -fill x  -side left
  pack configure .xfbarchartxhair.frame15.button10  -expand 1  -fill x  -side left
  pack configure .xfbarchartxhair.frame15.checkbutton16  -expand 1  -fill both  -side left
  pack configure .xfbarchartxhair.frame15.button12  -expand 1  -fill x  -side left

  # pack master .xfbarchartxhair.frame22
