  # build widget .xfbarchartlegend.frame15.button9
  button .xfbarchartlegend.frame15.button9  -command {set xfnewval "$xfbarchart_path legend configure"
foreach xfitem [array names xfbarchartlegendOptions] {
	lappend xfnewval $xfitem
	lappend xfnewval [string trim $xfbarchartlegendOptions($xfitem)]
      }
eval $xfnewval
xfbarchartdestroy.xfbarchartlegend}  -padx {11}  -pady {4}  -text {OK}

  # build widget .xfbarchartlegend.frame15.button10
  button .xfbarchartlegend.frame15.button10  -command {set xfnewval "$xfbarchart_path legend configure"
foreach xfitem [array names xfbarchartlegendOptions] {
	lappend xfnewval $xfitem
	lappend xfnewval [string trim $xfbarchartlegendOptions($xfitem)]
      }
eval $xfnewval}  -padx {11}  -pady {4}  -text {Apply}

