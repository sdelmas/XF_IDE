  entry .xfbarchartelem.frame7.frame38.entry  -textvariable {xfbarchartelemOptions(-ydata)}
  pack  .xfbarchartelem.frame7.frame38 -fill x
  pack  .xfbarchartelem.frame7.frame38.label -side left
  pack  .xfbarchartelem.frame7.frame38.entry -expand 1 -fill x -side left

  frame .xfbarchartelem.frame8  -height {30}  -width {30}
  button .xfbarchartelem.frame8.button9  -command {xfbarchartcfg_gen $xfbarchart_path}  -padx {11}  -pady {4}  -text {Parameters}

  button .xfbarchartelem.frame8.button10  -command {xfbarchartcfg_axis $xfbarchart_path}  -padx {11}  -pady {4}  -text {Axis}

  button .xfbarchartelem.frame8.button11  -padx {11}  -pady {4}  -text {Legend}

  button .xfbarchartelem.frame8.button12  -padx {11}  -pady {4}  -text {Crosshairs}

  button .xfbarchartelem.frame8.button13  -padx {11}  -pady {4}  -text {Postscript}

  frame .xfbarchartelem.frame15  -height {30}  -width {30}

  button .xfbarchartelem.frame15.button9  -command {set xfnewval "$xfbarchart_path element configure $xfbarchartcurelem"
foreach xfitem [lsort [array names xfbarchartelemOptions]] {
	lappend xfnewval $xfitem
	lappend xfnewval [string trim $xfbarchartelemOptions($xfitem)]
      }
eval $xfnewval
xfbarchartdestroy.xfbarchartelem}  -padx {11}  -pady {4}  -text {OK}

  # build widget .xfbarchartelem.frame15.button10
  button .xfbarchartelem.frame15.button10  -command {if {$xfbarchartcurelem != ""} {
	set xfnewval "$xfbarchart_path element configure $xfbarchartcurelem"
	foreach xfitem [lsort [array names xfbarchartelemOptions]] {
		lappend xfnewval $xfitem
		lappend xfnewval [string trim $xfbarchartelemOptions($xfitem)]
	      }
	eval $xfnewval
   }}  -padx {11}  -pady {4}  -text {Apply}

  # build widget .xfbarchartelem.frame15.button12
  button .xfbarchartelem.frame15.button12  -command {if {$xfbarchartcurelem != ""} {
	eval "$xfbarchart_path element configure $xfbarchartcurelem $xfbarchartelemDefaults"
   }
xfbarchartdestroy.xfbarchartelem}  -padx {11}  -pady {4}  -text {Cancel}

  # build widget .xfbarchartelem.frame15.checkbutton16
  checkbutton .xfbarchartelem.frame15.checkbutton16  -text {Apply permanently}  -variable {xfConf(applyParameters)}

  # build widget .xfbarchartelem.frame0
  frame .xfbarchartelem.frame0  -height {30}  -width {30}

  # build widget .xfbarchartelem.frame0.button3
  button .xfbarchartelem.frame0.button3  -command {set elemlist [$xfbarchart_path element names]
set ctr [llength $elemlist]
incr ctr
while {[lsearch $elemlist $ctr] != -1} {
	incr ctr
      }
$xfbarchart_path element create $ctr
xfbarchartcfg_elem $xfbarchart_path}  -padx {11}  -pady {4}  -text {Add Element}

  # build widget .xfbarchartelem.frame0.button2
  button .xfbarchartelem.frame0.button2  -command {if {$xfbarchartcurelem != ""} {
    $xfbarchart_path element delete $xfbarchartcurelem
    xfbarchartcfg_elem $xfbarchart_path
   }}  -padx {11}  -pady {4}  -text {Delete Element}

  # pack master .xfbarchartelem.frame8
  pack configure .xfbarchartelem.frame8.button9  -expand 1  -fill x  -side left
  pack configure .xfbarchartelem.frame8.button10  -expand 1  -fill x  -side left
  pack configure .xfbarchartelem.frame8.button11  -expand 1  -fill x  -side left
  pack configure .xfbarchartelem.frame8.button12  -expand 1  -fill x  -side left
  pack configure .xfbarchartelem.frame8.button13  -expand 1  -fill x  -side left

  # pack master .xfbarchartelem.frame15
