# procedure to show window xfbarchartshow.xfbarchartps
proc xfbarchartshow.xfbarchartps { args} {
  global xfBind

  # build widget .xfbarchartps
  catch "destroy .xfbarchartps"
  toplevel .xfbarchartps   -relief {raised}

  # Window manager configurations
  wm positionfrom .xfbarchartps ""
  wm sizefrom .xfbarchartps ""
  wm maxsize .xfbarchartps 1152 849
  wm minsize .xfbarchartps 104 10
  wm title .xfbarchartps {Barchart Postscript Configuration}


  # build widget .xfbarchartps.frame7
  frame .xfbarchartps.frame7  -borderwidth {2}  -height {30}  -relief {raised}  -width {30}

