# Program: xf
# Description: handle BLT 2.4 barchart widgets
#
# Created by Dennis LaBelle - 11/29/98
#
set default [pwd]
set blt_version 2.4
catch {load blt24}
catch {namespace import blt::*} result

global xfDefaultConf
set xfDefaultConf(barchart) 4

##########
# Procedure: XFAdd.Barchart
# Description: add a Barchart
# Arguments: xfW - the widget
#            xfName - a optional name
#            xfType - add or config
# Returns: none
# Sideeffects: none
##########
proc XFAdd.Barchart {xfW xfName xfType} {
  global xfStatus

  XFEditSetStatus "Inserting Barchart..."
  set xfName [XFMiscGetUniqueName $xfName barchart]
  if {"$xfStatus(path)" == "."} {
    if {"$xfType" == "add"} {
      if {[catch "barchart .$xfName" xfResult]} {
        XFProcError "$xfResult"
        XFEditSetStatus "Inserting Barchart...aborted"
        return
      }
    } {
      if {[catch "Barchart .$xfName" xfResult]} {
        XFProcError "$xfResult"
        XFEditSetStatus "Inserting Barchart...aborted"
        return
      }
    }

    XFMiscPositionWidget .$xfName
    XFMiscBindWidgetTree .$xfName
  } {
    if {"$xfType" == "add"} {
      if {[catch "barchart $xfStatus(path).$xfName" xfResult]} {
        XFProcError "$xfResult"
        XFEditSetStatus "Inserting Barchart...aborted"
        return
      }
    } {
      if {[catch "barchart $xfStatus(path).$xfName" xfResult]} {
        XFProcError "$xfResult"
        XFEditSetStatus "Inserting Barchart...aborted"
        return
      }
    }

    XFMiscPositionWidget $xfStatus(path).$xfName
    XFMiscBindWidgetTree $xfStatus(path).$xfName
  }

  incr xfStatus(elementCounter)
  XFEditSetPath $xfStatus(path)
  XFEditSetStatus "Inserting Barchart...done"
}

##########
# Procedure: XFConfig.Barchart4
# Description: configure a Barchart
# Arguments: xfW - the widget
#            xfType - config type (add or config)
#            xfClass - the class we configure
#            xfLeader - the leading window
# Returns: none
# Sideeffects: none
##########
proc XFConfig.Barchart4 {xfW xfType xfClass {xfLeader ""}} {
  global xfStatus

  if {"$xfType" == "add"} {
    set xfName barchart$xfStatus(elementCounter)
  } {
    set xfName [XFMiscPathName $xfW]
  }
  XFEditSetStatus "Calling parameter setting for Barchart..."

  xfbarchartcfg_gen $xfW
  if {[winfo exists .xfbarchartelem]} {xfbarchartcfg_elem $xfW}

  XFEditSetStatus "Calling parameter setting for Barchart...done"
}

##########
# Procedure: XFBarchartSetBarchart4
# Description: set barchart parameters
# Arguments: xfW - the widget
#            xfType - the type of setting (1 = set always, 0 = set
#                     only if permanent apply is on)
#            xfClass - the class we configure
#            xfParam1 - ignored parameter
# Returns: none
# Sideeffects: none
##########
proc XFBarchartSetBarchart4 {xfW xfType xfClass {xfParam1 ""}} {
  global xfConf
  global xfMisc

  if {$xfType == 0 && !$xfConf(applyParameters)} {
    return
  }
  if {"[info commands $xfW]" == ""} {
    return
  }
  XFMiscSetSymbolicName $xfW \
    [.xf${xfClass}Config4.params1.params2.symname.symname get]

  XFMiscSetResource $xfW background \
    [.xf${xfClass}Config4.params1.params2.bg.bg get]
}

##########
# Procedure: XFSaveSpecial.Barchart
# Description: save barbarchart
# Arguments: xfW - the widget
# Returns: none
# Sideeffects: none
##########
proc XFSaveSpecial.Barchart {xfW} {
global xfbarchartDefaults
global xfbarchartcurelem
global xfbarchartelemDefaults
global xfbarchartaxisDefaults
global xfbarchartlegendDefaults
global xfbarchartxhairDefaults
global xfbarchartpsDefaults
global xfbarchartelemOptions

  xfbarchart_getopt $xfW
  set rtnval "\n  $xfW configure $xfbarchartDefaults\n"
  set vnamelist [vector names]
  set elements [lsort [$xfW element names]]
  foreach element $elements {
	    if {$element != ""} {
			xfbarchart_getelemopt $xfW $element
			set vname $xfbarchartelemOptions(-xdata)
			if {[lsearch $vnamelist $vname] != -1} {
				# Output code to support vector
				upvar #0 $vname vecname
				set rtnval "$rtnval\nglobal $vname"
				set rtnval "$rtnval\nif {\[info exists $vname\]} {unset $vname}"
				set rtnval "$rtnval\nvector create $vname"
				set rtnval "$rtnval\n$vname set {$vecname(:)}"
			   }
			set vname $xfbarchartelemOptions(-ydata)
			if {[lsearch $vnamelist $vname] != -1} {
				# Output code to support vector
				upvar #0 $vname vecname
				set rtnval "$rtnval\nglobal $vname"
				set rtnval "$rtnval\nif {\[info exists $vname\]} {unset $vname}"
				set rtnval "$rtnval\nvector create $vname"
				set rtnval "$rtnval\n$vname set {$vecname(:)}"
			   }
			set rtnval "$rtnval\n  $xfW element create $element $xfbarchartelemDefaults\n"
		 }
	  }
  xfbarchart_getaxisopt $xfW xaxis
  set rtnval "$rtnval\n  $xfW xaxis configure $xfbarchartaxisDefaults\n"
  xfbarchart_getaxisopt $xfW yaxis
  set rtnval "$rtnval\n  $xfW yaxis configure $xfbarchartaxisDefaults\n"
  xfbarchart_getaxisopt $xfW x2axis
  set rtnval "$rtnval\n  $xfW x2axis configure $xfbarchartaxisDefaults\n"
  xfbarchart_getaxisopt $xfW y2axis
  set rtnval "$rtnval\n  $xfW y2axis configure $xfbarchartaxisDefaults\n"
  xfbarchart_getlegendopt $xfW
  set rtnval "$rtnval\n  $xfW legend configure $xfbarchartlegendDefaults\n"
  xfbarchart_getxhairopt $xfW
  set rtnval "$rtnval\n  $xfW crosshairs configure $xfbarchartxhairDefaults\n"
  xfbarchart_getpsopt $xfW
  set rtnval "$rtnval\n  $xfW postscript configure $xfbarchartpsDefaults\n"
  return $rtnval
}
# procedure to show window xfbarchartshow.xfbarchartaxis
proc xfbarchartshow.xfbarchartaxis { args} {
  global xfBind

  # build widget .xfbarchartaxis
  catch "destroy .xfbarchartaxis"
