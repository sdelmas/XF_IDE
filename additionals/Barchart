     catch "destroy .xfbarchartelem"
}


# procedure to show window xfbarchartshow.xfbarchartgen
proc xfbarchartshow.xfbarchartgen { args} {
  global xfBind

  # build widget .xfbarchartgen
  catch "destroy .xfbarchartgen"
  toplevel .xfbarchartgen   -relief {raised}

  # Window manager configurations
  wm positionfrom .xfbarchartgen ""
  wm sizefrom .xfbarchartgen ""
  wm maxsize .xfbarchartgen 1152 849
  wm minsize .xfbarchartgen 104 10
  wm title .xfbarchartgen {Barchart Parameters}


  # build widget .xfbarchartgen.frame7
  frame .xfbarchartgen.frame7  -borderwidth {2}  -height {30}  -relief {raised}  -width {30}

  # build widget .xfbarchartgen.frame7.frame17
  frame .xfbarchartgen.frame7.frame17  -height {30}  -width {30}

  # build widget .xfbarchartgen.frame7.frame17.label18
  label .xfbarchartgen.frame7.frame17.label18  -borderwidth {0}  -text {Background:}  -width {16} -anchor e

  # build widget .xfbarchartgen.frame7.frame17.entry19
  entry .xfbarchartgen.frame7.frame17.entry19  -textvariable {xfbarchart(-background)}
  # bindings
  bind .xfbarchartgen.frame7.frame17.entry19 $xfBind(configure) {set xfbarchartnewval [tk_chooseColor -parent .xfbarchartgen]
if {$xfbarchartnewval != ""} {set xfbarchart(-background) $xfbarchartnewval}}

  # build widget .xfbarchartgen.frame7.frame26
  frame .xfbarchartgen.frame7.frame26  -height {30}  -width {30}

  # build widget .xfbarchartgen.frame7.frame26.label18
  label .xfbarchartgen.frame7.frame26.label18  -borderwidth {0}  -text {Borderwidth:}  -width {16} -anchor e

