foreach xfitem [array names xfbarchartpsOptions] {
	lappend xfnewval $xfitem
	lappend xfnewval [string trim $xfbarchartpsOptions($xfitem)]
      }
eval $xfnewval}  -padx {11}  -pady {4}  -text {Apply}

  # build widget .xfbarchartps.frame15.button12
