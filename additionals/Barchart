# Procedure: xfbarchart_getaxisopt
proc xfbarchart_getaxisopt { barchart_path axis} {
    global xfbarchartaxisOptions xfbarchartaxisDefaults

    foreach item [$barchart_path $axis configure] {
	if {[llength $item] < 4} {
	    continue
	}
	set name [lindex $item 0] 
	set value [lindex $item 4]
	set xfbarchartaxisOptions($name) $value
    }
    set names [lsort [array names xfbarchartaxisOptions]]
    set xfbarchartaxisDefaults {}
    foreach i $names {
	lappend xfbarchartaxisDefaults $i $xfbarchartaxisOptions($i)
    }
}


# Procedure: xfbarchart_getelemopt
proc xfbarchart_getelemopt { barchart_path element} {
    global xfbarchartelemOptions xfbarchartelemDefaults xfbarchartcurelem

    set xfbarchartcurelem $element
    foreach item [$barchart_path element configure $element] {
	if {[llength $item] < 4} {
	    continue
	}
	set name [lindex $item 0] 
	set value [lindex $item 4]
	set xfbarchartelemOptions($name) $value
    }
    set names [lsort [array names xfbarchartelemOptions]]
    set xfbarchartelemDefaults {}
    foreach i $names {
	lappend xfbarchartelemDefaults $i $xfbarchartelemOptions($i)
    }
}


# Procedure: xfbarchart_getlegendopt
proc xfbarchart_getlegendopt { barchart_path} {
    global xfbarchartlegendOptions xfbarchartlegendDefaults

    foreach item [$barchart_path legend configure] {
	if {[llength $item] < 4} {
	    continue
	}
	set name [lindex $item 0] 
	set value [lindex $item 4]
	set xfbarchartlegendOptions($name) $value
    }
    set names [lsort [array names xfbarchartlegendOptions]]
    set xfbarchartlegendDefaults {}
    foreach i $names {
	lappend xfbarchartlegendDefaults $i $xfbarchartlegendOptions($i)
    }
}


# Procedure: xfbarchart_getopt
proc xfbarchart_getopt { barchart_path} {
    global xfbarchart xfbarchartDefaults

    foreach item [$barchart_path configure] {
	if {[llength $item] < 4} {
	    continue
	}
	set name [lindex $item 0] 
	set value [lindex $item 4]
	set xfbarchart($name) $value
    }
    set names [lsort [array names xfbarchart]]
    set xfbarchartDefaults {}
    foreach i $names {
	lappend xfbarchartDefaults $i $xfbarchart($i)
    }
}


# Procedure: xfbarchart_getpsopt
proc xfbarchart_getpsopt { barchart_path} {
    global xfbarchartpsOptions xfbarchartpsDefaults

    foreach item [$barchart_path postscript configure] {
	if {[llength $item] < 4} {
	    continue
	}
	set name [lindex $item 0] 
	set value [lindex $item 4]
	set xfbarchartpsOptions($name) $value
    }
    set names [lsort [array names xfbarchartpsOptions]]
    set xfbarchartpsDefaults {}
    foreach i $names {
	lappend xfbarchartpsDefaults $i $xfbarchartpsOptions($i)
    }
}


# Procedure: xfbarchart_getxhairopt
proc xfbarchart_getxhairopt { barchart_path} {
    global xfbarchartxhairOptions xfbarchartxhairDefaults

    foreach item [$barchart_path crosshairs configure] {
	if {[llength $item] < 4} {
	    continue
	}
	set name [lindex $item 0] 
	set value [lindex $item 4]
	set xfbarchartxhairOptions($name) $value
    }
    set names [lsort [array names xfbarchartxhairOptions]]
    set xfbarchartxhairDefaults {}
    foreach i $names {
	lappend xfbarchartxhairDefaults $i $xfbarchartxhairOptions($i)
    }
}


# Procedure: xfbarchartcfg_axis
proc xfbarchartcfg_axis { barchart_path} {
    global xfbarchartaxisOptions xfbarchartaxisDefaults 
    global xfbarchart_parent xfbarchart_name xfbarchartcuraxis xfbarchart_path

    set xfbarchart_parent [winfo parent $barchart_path]
    set xfbarchart_name [winfo name $barchart_path]
    set xfbarchart_path $barchart_path

    xfbarchart_getaxisopt $barchart_path xaxis
    xfbarchartshow.xfbarchartaxis
    set xfbarchartcuraxis xaxis
}


# Procedure: xfbarchartcfg_elem
proc xfbarchartcfg_elem { barchart_path} {
    global xfbarchartelemOptions xfbarchartelemDefaults
    global xfbarchart_parent xfbarchart_name xfbarchartcurelem xfbarchart_path

    set xfbarchart_parent [winfo parent $barchart_path]
    set xfbarchart_name [winfo name $barchart_path]
    set xfbarchart_path $barchart_path

    set elements [lsort [$barchart_path element names]]
    if	{ [llength $elements] > 0 } {
	  set element [lindex $elements 0]
	  xfbarchart_getelemopt $barchart_path $element
	} {
	  set xfbarchartcurelem ""
	}

    xfbarchartshow.xfbarchartelem
    update

    set count 0
    if {[winfo exists .xfbarchartelem.frame7.elemnames.frame]} {destroy .xfbarchartelem.frame7.elemnames.frame}
    frame .xfbarchartelem.frame7.elemnames.frame
    pack .xfbarchartelem.frame7.elemnames.frame
    foreach i $elements {
	radiobutton .xfbarchartelem.frame7.elemnames.frame.rbutton_$i -variable xfbarchartcurelem -value $i -text $i  -command "xfbarchart_getelemopt $barchart_path $i"
	pack .xfbarchartelem.frame7.elemnames.frame.rbutton_$i -side left
	incr count
    }
}


# Procedure: xfbarchartcfg_gen
proc xfbarchartcfg_gen { barchart_path} {
    global xfbarchart xfbarchartDefaults
    global xfbarchart_parent xfbarchart_name xfbarchart_path

    set xfbarchart_parent [winfo parent $barchart_path]
    set xfbarchart_name [winfo name $barchart_path]
    set xfbarchart_path $barchart_path

    xfbarchart_getopt $barchart_path
    xfbarchartshow.xfbarchartgen
}


# Procedure: xfbarchartcfg_legend
proc xfbarchartcfg_legend { barchart_path} {
    global xfbarchartlegendOptions xfbarchartlegendDefaults
    global xfbarchart_parent xfbarchart_name xfbarchart_path

    set xfbarchart_parent [winfo parent $barchart_path]
    set xfbarchart_name [winfo name $barchart_path]
    set xfbarchart_path $barchart_path

    xfbarchart_getlegendopt $barchart_path
    xfbarchartshow.xfbarchartlegend
}


# Procedure: xfbarchartcfg_ps
proc xfbarchartcfg_ps { barchart_path} {
    global xfbarchartpsOptions xfbarchartpsDefaults
    global xfbarchart_parent xfbarchart_name xfbarchart_path

    set xfbarchart_parent [winfo parent $barchart_path]
    set xfbarchart_name [winfo name $barchart_path]
    set xfbarchart_path $barchart_path

    xfbarchart_getpsopt $barchart_path
    xfbarchartshow.xfbarchartps
}


# Procedure: xfbarchartcfg_xhair
proc xfbarchartcfg_xhair { barchart_path} {
    global xfbarchartxhairOptions xfbarchartxhairDefaults
    global xfbarchart_parent xfbarchart_name xfbarchart_path

    set xfbarchart_parent [winfo parent $barchart_path]
    set xfbarchart_name [winfo name $barchart_path]
    set xfbarchart_path $barchart_path

    xfbarchart_getxhairopt $barchart_path
    xfbarchartshow.xfbarchartxhair
}

