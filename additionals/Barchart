  # build widget .xfbarchartaxis.frame15
  frame .xfbarchartaxis.frame15  -height {30}  -width {30}

  # build widget .xfbarchartaxis.frame15.button9
  button .xfbarchartaxis.frame15.button9  -command {set xfnewval "$xfbarchart_path $xfbarchartcuraxis configure"
foreach xfitem [array names xfbarchartaxisOptions] {
	lappend xfnewval $xfitem
	lappend xfnewval [string trim $xfbarchartaxisOptions($xfitem)]
      }
eval $xfnewval
xfbarchartdestroy.xfbarchartaxis}  -padx {11}  -pady {4}  -text {OK}

  # build widget .xfbarchartaxis.frame15.button10
  button .xfbarchartaxis.frame15.button10  -command {set xfnewval "$xfbarchart_path $xfbarchartcuraxis configure"
foreach xfitem [array names xfbarchartaxisOptions] {
	lappend xfnewval $xfitem
	lappend xfnewval [string trim $xfbarchartaxisOptions($xfitem)]
      }
eval $xfnewval}  -padx {11}  -pady {4}  -text {Apply}

  # build widget .xfbarchartaxis.frame15.button12
  button .xfbarchartaxis.frame15.button12  -command {eval "$xfbarchart_path $xfbarchartcuraxis configure $xfbarchartaxisDefaults"
xfbarchartdestroy.xfbarchartaxis}  -padx {11}  -pady {4}  -text {Cancel}

  # build widget .xfbarchartaxis.frame15.checkbutton16
  checkbutton .xfbarchartaxis.frame15.checkbutton16  -text {Apply permanently}  -variable {xfConf(applyParameters)}

  # build widget .xfbarchartaxis.frame22
  frame .xfbarchartaxis.frame22  -borderwidth {2}  -height {30}  -relief {raised}  -width {30}

  # build widget .xfbarchartaxis.frame22.label21
  label .xfbarchartaxis.frame22.label21  -borderwidth {0}  -text {Widget path:}

  # build widget .xfbarchartaxis.frame22.label25
  label .xfbarchartaxis.frame22.label25  -borderwidth {0}  -text {.frame0}  -textvariable {xfbarchart_parent}

  # pack master .xfbarchartaxis.frame7
  pack configure .xfbarchartaxis.frame7.frame22  -fill x
  pack configure .xfbarchartaxis.frame7.elemnames  -fill x

  pack  .xfbarchartaxis.frame7.frame200 -fill x
  pack  .xfbarchartaxis.frame7.frame200.label -side left
  pack  .xfbarchartaxis.frame7.frame200.entry -expand 1 -fill x -side left

  pack configure .xfbarchartaxis.frame7.frame32  -fill x
  pack configure .xfbarchartaxis.frame7.frame40  -fill x
  pack configure .xfbarchartaxis.frame7.frame15  -fill x
  pack configure .xfbarchartaxis.frame7.frame27  -fill x
  pack configure .xfbarchartaxis.frame7.frame6  -fill x
  pack configure .xfbarchartaxis.frame7.frame8  -fill x
  pack configure .xfbarchartaxis.frame7.frame10  -fill x
  pack configure .xfbarchartaxis.frame7.frame28  -fill x
  pack configure .xfbarchartaxis.frame7.frame29  -fill x
  pack configure .xfbarchartaxis.frame7.frame30  -fill x

  pack  .xfbarchartaxis.frame7.frame201 -fill x
  pack  .xfbarchartaxis.frame7.frame201.label -side left
  pack  .xfbarchartaxis.frame7.frame201.entry -expand 1 -fill x -side left

  pack configure .xfbarchartaxis.frame7.frame12  -fill x
  pack configure .xfbarchartaxis.frame7.frame31  -fill x
  pack configure .xfbarchartaxis.frame7.frame33  -fill x
  pack configure .xfbarchartaxis.frame7.frame34  -fill x
  pack configure .xfbarchartaxis.frame7.frame35  -fill x
  pack configure .xfbarchartaxis.frame7.frame26  -fill x

  # pack master .xfbarchartaxis.frame7.frame22
