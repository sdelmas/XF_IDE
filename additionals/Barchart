}


# procedure to show window xfbarchartshow.xfbarchartlegend
proc xfbarchartshow.xfbarchartlegend { args} {
  global xfBind

  # build widget .xfbarchartlegend
  catch "destroy .xfbarchartlegend"
  toplevel .xfbarchartlegend   -relief {raised}

  # Window manager configurations
  wm positionfrom .xfbarchartlegend ""
  wm sizefrom .xfbarchartlegend ""
  wm maxsize .xfbarchartlegend 1152 849
  wm minsize .xfbarchartlegend 104 1
  wm title .xfbarchartlegend {Barchart Legend Configuration Options}


  # build widget .xfbarchartlegend.frame7
  frame .xfbarchartlegend.frame7  -borderwidth {2}  -height {30}  -relief {raised}  -width {30}

  # build widget .xfbarchartlegend.frame7.frame17
  frame .xfbarchartlegend.frame7.frame17  -height {30}  -width {30}

  # build widget .xfbarchartlegend.frame7.frame17.label18
  label .xfbarchartlegend.frame7.frame17.label18  -borderwidth {0}  -text {Activebackground:}  -width {16} -anchor e

  # build widget .xfbarchartlegend.frame7.frame17.entry19
  entry .xfbarchartlegend.frame7.frame17.entry19  -textvariable {xfbarchartlegendOptions(-activebackground)}
  # bindings
  bind .xfbarchartlegend.frame7.frame17.entry19 $xfBind(configure) {set xfbarchartnewval [tk_chooseColor -parent .xfbarchartlegend]
if {$xfbarchartnewval != ""} {set xfbarchartlegendOptions(-activebackground) $xfbarchartnewval}}

  # build widget .xfbarchartlegend.frame7.frame22
  frame .xfbarchartlegend.frame7.frame22  -height {30}  -width {30}

  # build widget .xfbarchartlegend.frame7.frame22.label21
  label .xfbarchartlegend.frame7.frame22.label21  -borderwidth {0}  -text {Name:}  -width {16} -anchor e

  # build widget .xfbarchartlegend.frame7.frame22.label25
  label .xfbarchartlegend.frame7.frame22.label25  -borderwidth {0}  -text {barchart0}  -textvariable {xfbarchart_name}

  # build widget .xfbarchartlegend.frame7.frame26
  frame .xfbarchartlegend.frame7.frame26  -height {30}  -width {30}

  # build widget .xfbarchartlegend.frame7.frame26.label18
