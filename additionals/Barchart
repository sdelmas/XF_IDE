  radiobutton .xfbarchartelem.frame7.frame36.radiobutton1  -highlightthickness {0}  -pady {0}  -text {Ridge}  -value {ridge}  -variable {xfbarchartelemOptions(-relief)}  -width {7} -anchor w
  radiobutton .xfbarchartelem.frame7.frame36.radiobutton2  -highlightthickness {0}  -pady {0}  -text {Groove}  -value {groove}  -variable {xfbarchartelemOptions(-relief)}  -width {7} -anchor w
