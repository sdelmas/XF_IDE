  frame .xfbarchartaxis.frame7.frame200  -height {30}  -width {30}
  label .xfbarchartaxis.frame7.frame200.label  -borderwidth {0}  -text {Autorange:} -width {16} -anchor e
  entry .xfbarchartaxis.frame7.frame200.entry  -textvariable {xfbarchartaxisOptions(-autorange)}

  # build widget .xfbarchartaxis.frame7.frame26
