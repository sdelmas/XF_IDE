# procedure to show window xfstripchartshow.xfstripchartps
proc xfstripchartshow.xfstripchartps { args} {
  global xfBind

  # build widget .xfstripchartps
  catch "destroy .xfstripchartps"
  toplevel .xfstripchartps   -relief {raised}

  # Window manager configurations
  wm positionfrom .xfstripchartps ""
  wm sizefrom .xfstripchartps ""
  wm maxsize .xfstripchartps 1152 849
  wm minsize .xfstripchartps 104 10
  wm title .xfstripchartps {Stripchart Postscript Configuration}


  # build widget .xfstripchartps.frame7
