# procedure to show window xfstripchartshow.xfstripchartlegend
proc xfstripchartshow.xfstripchartlegend { args} {
  global xfBind

  # build widget .xfstripchartlegend
  catch "destroy .xfstripchartlegend"
  toplevel .xfstripchartlegend   -relief {raised}

  # Window manager configurations
  wm positionfrom .xfstripchartlegend ""
  wm sizefrom .xfstripchartlegend ""
  wm maxsize .xfstripchartlegend 1152 849
  wm minsize .xfstripchartlegend 104 1
  wm title .xfstripchartlegend {Stripchart Legend Configuration Options}


  # build widget .xfstripchartlegend.frame7
  frame .xfstripchartlegend.frame7  -borderwidth {2}  -height {30}  -relief {raised}  -width {30}

  # build widget .xfstripchartlegend.frame7.frame17
  frame .xfstripchartlegend.frame7.frame17  -height {30}  -width {30}

  # build widget .xfstripchartlegend.frame7.frame17.label18
  label .xfstripchartlegend.frame7.frame17.label18  -borderwidth {0}  -text {Activebackground:}  -width {16} -anchor e

  # build widget .xfstripchartlegend.frame7.frame17.entry19
  entry .xfstripchartlegend.frame7.frame17.entry19  -textvariable {xfstripchartlegendOptions(-activebackground)}
  # bindings
  bind .xfstripchartlegend.frame7.frame17.entry19 $xfBind(configure) {set xfstripchartnewval [tk_chooseColor -parent .xfstripchartlegend]
if {$xfstripchartnewval != ""} {set xfstripchartlegendOptions(-activebackground) $xfstripchartnewval}}

  # build widget .xfstripchartlegend.frame7.frame22
  frame .xfstripchartlegend.frame7.frame22  -height {30}  -width {30}

  # build widget .xfstripchartlegend.frame7.frame22.label21
  label .xfstripchartlegend.frame7.frame22.label21  -borderwidth {0}  -text {Name:}  -width {16} -anchor e

  # build widget .xfstripchartlegend.frame7.frame22.label25
  label .xfstripchartlegend.frame7.frame22.label25  -borderwidth {0}  -text {stripchart0}  -textvariable {xfstripchart_name}

  # build widget .xfstripchartlegend.frame7.frame26
  frame .xfstripchartlegend.frame7.frame26  -height {30}  -width {30}

  # build widget .xfstripchartlegend.frame7.frame26.label18
