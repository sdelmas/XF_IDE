  # build widget .xfstripchartelem.frame8.button10
  button .xfstripchartelem.frame8.button10  -command {xfstripchartcfg_axis $xfstripchart_path}  -padx {11}  -pady {4}  -text {Axis}

  # build widget .xfstripchartelem.frame8.button11
  button .xfstripchartelem.frame8.button11  -padx {11}  -pady {4}  -text {Legend}

  # build widget .xfstripchartelem.frame8.button12
