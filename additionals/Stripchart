# procedure to show window xfstripchartshow.xfstripchartconf
proc xfstripchartshow.xfstripchartconf { args} {
  global xfBind

  # build widget .xfstripchartconf
  catch "destroy .xfstripchartconf"
  toplevel .xfstripchartconf 

  # Window manager configurations

  wm positionfrom .xfstripchartconf ""
  wm sizefrom .xfstripchartconf ""
  wm maxsize .xfstripchartconf 1156 849
  wm minsize .xfstripchartconf 104 1
  wm title .xfstripchartconf {Stripchart Configure Tool}

  # build widget .xfstripchartconf.cbutton_General
  checkbutton .xfstripchartconf.cbutton_General  -command {ConfigureGeneral}  -text {General}  -variable {varGeneral}

  # build widget .xfstripchartconf.cbutton_Element
  checkbutton .xfstripchartconf.cbutton_Element  -command {ConfigureElement}  -text {Element}  -variable {varElement}

  # build widget .xfstripchartconf.cbutton_Axis
  checkbutton .xfstripchartconf.cbutton_Axis  -command {ConfigureAxis}  -text {Axis}  -variable {varAxis}

  # build widget .xfstripchartconf.cbutton_Legend
  checkbutton .xfstripchartconf.cbutton_Legend  -command {ConfigureLegend}  -text {Legend}  -variable {varLegend}

  # build widget .xfstripchartconf.cbutton_Crosshairs
  checkbutton .xfstripchartconf.cbutton_Crosshairs  -command {ConfigureCrosshairs}  -text {Crosshairs}  -variable {varCrosshairs}

  # build widget .xfstripchartconf.cbutton_PostScript
  checkbutton .xfstripchartconf.cbutton_PostScript  -command {ConfigurePostScript}  -text {PostScript}  -variable {varPostScript}

  # build widget .xfstripchartconf.quit
  button .xfstripchartconf.quit  -command {xfstripchartdestroy.xfstripchartconf}  -padx {11}  -pady {4}  -text {quit}

  # build widget .xfstripchartconf.frame0
  frame .xfstripchartconf.frame0  -borderwidth {2}  -height {30}  -relief {ridge}  -width {30}

  # build widget .xfstripchartconf.frame0.label1
  label .xfstripchartconf.frame0.label1  -background {steelblue}  -font {helvetica 12 bold}  -foreground {white}  -relief {groove}  -text {Path:}

  # build widget .xfstripchartconf.frame0.entry2
  entry .xfstripchartconf.frame0.entry2  -background {lightblue}  -font {helvetica 12 bold}

  # pack master .xfstripchartconf.frame0
  pack configure .xfstripchartconf.frame0.label1  -fill y  -side left
  pack configure .xfstripchartconf.frame0.entry2  -expand 1  -fill x  -side left

  # pack master .xfstripchartconf
  pack configure .xfstripchartconf.frame0  -fill x
  pack configure .xfstripchartconf.cbutton_General  -anchor w
  pack configure .xfstripchartconf.cbutton_Element  -anchor w
  pack configure .xfstripchartconf.cbutton_Axis  -anchor w
  pack configure .xfstripchartconf.cbutton_Legend  -anchor w
  pack configure .xfstripchartconf.cbutton_Crosshairs  -anchor w
  pack configure .xfstripchartconf.cbutton_PostScript  -anchor w
  pack configure .xfstripchartconf.quit

  .xfstripchartconf.frame0.entry2 insert end {.frame0.xfstripchart0}
}

proc xfstripchartdestroy.xfstripchartconf {} {# xf ignore me 7
    catch "destroy .xfstripchartconf"
}


