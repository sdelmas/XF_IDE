  # build widget .xfstripchartelem.frame8.button13
  button .xfstripchartelem.frame8.button13  -padx {11}  -pady {4}  -text {Postscript}

  # build widget .xfstripchartelem.frame15
  frame .xfstripchartelem.frame15  -height {30}  -width {30}

  # build widget .xfstripchartelem.frame15.button9
  button .xfstripchartelem.frame15.button9  -command {set xfnewval "$xfstripchart_path element configure $xfstripchartcurelem"
foreach xfitem [lsort [array names xfstripchartelemOptions]] {
	lappend xfnewval $xfitem
	lappend xfnewval [string trim $xfstripchartelemOptions($xfitem)]
      }
eval $xfnewval
xfstripchartdestroy.xfstripchartelem}  -padx {11}  -pady {4}  -text {OK}

  # build widget .xfstripchartelem.frame15.button10
  button .xfstripchartelem.frame15.button10  -command {if {$xfstripchartcurelem != ""} {
	set xfnewval "$xfstripchart_path element configure $xfstripchartcurelem"
	foreach xfitem [lsort [array names xfstripchartelemOptions]] {
		lappend xfnewval $xfitem
		lappend xfnewval [string trim $xfstripchartelemOptions($xfitem)]
	      }
	eval $xfnewval
   }}  -padx {11}  -pady {4}  -text {Apply}

  # build widget .xfstripchartelem.frame15.button12
  button .xfstripchartelem.frame15.button12  -command {if {$xfstripchartcurelem != ""} {
	eval "$xfstripchart_path element configure $xfstripchartcurelem $xfstripchartelemDefaults"
   }
xfstripchartdestroy.xfstripchartelem}  -padx {11}  -pady {4}  -text {Cancel}

  # build widget .xfstripchartelem.frame15.checkbutton16
  checkbutton .xfstripchartelem.frame15.checkbutton16  -text {Apply permanently}  -variable {xfConf(applyParameters)}

  # build widget .xfstripchartelem.frame22
  frame .xfstripchartelem.frame22  -borderwidth {2}  -height {30}  -relief {raised}  -width {30}

  # build widget .xfstripchartelem.frame22.label21
  label .xfstripchartelem.frame22.label21  -borderwidth {0}  -text {Widget path:}

  # build widget .xfstripchartelem.frame22.label25
  label .xfstripchartelem.frame22.label25  -borderwidth {0}  -text {.frame0}  -textvariable {xfstripchart_parent}

  # build widget .xfstripchartelem.frame0
  frame .xfstripchartelem.frame0  -height {30}  -width {30}

  # build widget .xfstripchartelem.frame0.button3
  button .xfstripchartelem.frame0.button3  -command {set elemlist [$xfstripchart_path element names]
set ctr [llength $elemlist]
incr ctr
while {[lsearch $elemlist $ctr] != -1} {
	incr ctr
      }
$xfstripchart_path element create $ctr
xfstripchartcfg_elem $xfstripchart_path}  -padx {11}  -pady {4}  -text {Add Element}

  # build widget .xfstripchartelem.frame0.button2
  button .xfstripchartelem.frame0.button2  -command {if {$xfstripchartcurelem != ""} {
    $xfstripchart_path element delete $xfstripchartcurelem
    xfstripchartcfg_elem $xfstripchart_path
   }}  -padx {11}  -pady {4}  -text {Delete Element}

  # pack master .xfstripchartelem.frame7
  pack configure .xfstripchartelem.frame7.frame22  -fill x
  pack configure .xfstripchartelem.frame7.elemnames  -fill x
  pack configure .xfstripchartelem.frame7.frame17  -fill x
  pack configure .xfstripchartelem.frame7.frame26  -fill x
