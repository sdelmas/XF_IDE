      }
eval $xfnewval}  -padx {11}  -pady {4}  -text {Apply}

  # build widget .xfstripchartaxis.frame15.button12
  button .xfstripchartaxis.frame15.button12  -command {eval "$xfstripchart_path $xfstripchartcuraxis configure $xfstripchartaxisDefaults"
xfstripchartdestroy.xfstripchartaxis}  -padx {11}  -pady {4}  -text {Cancel}

  # build widget .xfstripchartaxis.frame15.checkbutton16
