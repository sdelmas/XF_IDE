foreach xfitem [array names xfstripchartaxisOptions] {
	lappend xfnewval $xfitem
	lappend xfnewval [string trim $xfstripchartaxisOptions($xfitem)]
      }
eval $xfnewval
xfstripchartdestroy.xfstripchartaxis}  -padx {11}  -pady {4}  -text {OK}

