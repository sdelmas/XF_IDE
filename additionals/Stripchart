if {$xfstripchartnewval != ""} {set xfstripchartlegendOptions(-activeforeground) $xfstripchartnewval}}

  # build widget .xfstripchartlegend.frame7.frame28
  frame .xfstripchartlegend.frame7.frame28  -height {30}  -width {30}

  # build widget .xfstripchartlegend.frame7.frame28.label18
  label .xfstripchartlegend.frame7.frame28.label18  -borderwidth {0}  -text {Background:}  -width {16} -anchor e

  # build widget .xfstripchartlegend.frame7.frame28.entry19
  entry .xfstripchartlegend.frame7.frame28.entry19  -textvariable {xfstripchartlegendOptions(-background)}
  # bindings
