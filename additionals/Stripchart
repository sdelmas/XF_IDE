# procedure to show window xfstripchartshow.xfstripchartgen
proc xfstripchartshow.xfstripchartgen { args} {
  global xfBind

  # build widget .xfstripchartgen
  catch "destroy .xfstripchartgen"
  toplevel .xfstripchartgen   -relief {raised}

  # Window manager configurations
  wm positionfrom .xfstripchartgen ""
  wm sizefrom .xfstripchartgen ""
  wm maxsize .xfstripchartgen 1152 849
  wm minsize .xfstripchartgen 104 10
  wm title .xfstripchartgen {Stripchart Parameters}


  # build widget .xfstripchartgen.frame7
  frame .xfstripchartgen.frame7  -borderwidth {2}  -height {30}  -relief {raised}  -width {30}

  # build widget .xfstripchartgen.frame7.frame17
  frame .xfstripchartgen.frame7.frame17  -height {30}  -width {30}

  # build widget .xfstripchartgen.frame7.frame17.label18
  label .xfstripchartgen.frame7.frame17.label18  -borderwidth {0}  -text {Background:}  -width {16} -anchor e

  # build widget .xfstripchartgen.frame7.frame17.entry19
  entry .xfstripchartgen.frame7.frame17.entry19  -textvariable {xfstripchart(-background)}
  # bindings
  bind .xfstripchartgen.frame7.frame17.entry19 $xfBind(configure) {set xfstripchartnewval [tk_chooseColor -parent .xfstripchartgen]
if {$xfstripchartnewval != ""} {set xfstripchart(-background) $xfstripchartnewval}}

  # build widget .xfstripchartgen.frame7.frame26
  frame .xfstripchartgen.frame7.frame26  -height {30}  -width {30}

  # build widget .xfstripchartgen.frame7.frame26.label18
  label .xfstripchartgen.frame7.frame26.label18  -borderwidth {0}  -text {Borderwidth:}  -width {16} -anchor e

  # build widget .xfstripchartgen.frame7.frame26.entry19
  entry .xfstripchartgen.frame7.frame26.entry19  -textvariable {xfstripchart(-borderwidth)}

  # build widget .xfstripchartgen.frame7.frame27
  frame .xfstripchartgen.frame7.frame27  -height {30}  -width {30}

  # build widget .xfstripchartgen.frame7.frame27.label18
  label .xfstripchartgen.frame7.frame27.label18  -borderwidth {0}  -text {Bottommargin:}  -width {16} -anchor e

  # build widget .xfstripchartgen.frame7.frame27.entry19
