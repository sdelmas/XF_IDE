  pack configure .xfstripchartaxis.frame7.frame26.entry19  -expand 1  -fill x  -side left

  # pack master .xfstripchartaxis.frame7.frame27
  pack configure .xfstripchartaxis.frame7.frame27.label18  -side left
  pack configure .xfstripchartaxis.frame7.frame27.entry19  -expand 1  -fill x  -side left

  # pack master .xfstripchartaxis.frame7.frame28
  pack configure .xfstripchartaxis.frame7.frame28.label18  -side left
