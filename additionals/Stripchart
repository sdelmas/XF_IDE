	lappend xfstripchartDefaults $xfitem
	lappend xfstripchartDefaults [string trim $xfstripchart($xfitem)]
      }
eval "$xfstripchart_path configure $xfstripchartDefaults"
xfstripchartdestroy.xfstripchartgen}  -padx {11}  -pady {4}  -text {OK}

  # build widget .xfstripchartgen.frame15.button10
  button .xfstripchartgen.frame15.button10  -command {set xfnewval "$xfstripchart_path configure"
foreach xfitem [array names xfstripchart] {
	lappend xfnewval $xfitem
	lappend xfnewval [string trim $xfstripchart($xfitem)]
      }
eval $xfnewval}  -padx {11}  -pady {4}  -text {Apply}

  # build widget .xfstripchartgen.frame15.button12
  button .xfstripchartgen.frame15.button12  -command {eval "$xfstripchart_path configure $xfstripchartDefaults"
xfstripchartdestroy.xfstripchartgen}  -padx {11}  -pady {4}  -text {Cancel}

  # build widget .xfstripchartgen.frame15.checkbutton16
