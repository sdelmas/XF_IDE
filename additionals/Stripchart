  # bindings
  bind .xfstripchartgen.frame7.frame30.entry19 $xfBind(configure) {set xfstripchartnewval [tk_chooseColor -parent .xfstripchartgen]
if {$xfstripchartnewval != ""} {set xfstripchart(-foreground) $xfstripchartnewval}}

  # build widget .xfstripchartgen.frame7.frame31
  frame .xfstripchartgen.frame7.frame31  -height {30}  -width {30}

