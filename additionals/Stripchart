# Procedure: xfstripchart_getaxisopt
proc xfstripchart_getaxisopt { stripchart_path axis} {
    global xfstripchartaxisOptions xfstripchartaxisDefaults

    foreach item [$stripchart_path $axis configure] {
	if {[llength $item] < 4} {
	    continue
	}
	set name [lindex $item 0] 
	set value [lindex $item 4]
	set xfstripchartaxisOptions($name) $value
    }
    set names [lsort [array names xfstripchartaxisOptions]]
    set xfstripchartaxisDefaults {}
    foreach i $names {
	lappend xfstripchartaxisDefaults $i $xfstripchartaxisOptions($i)
    }
}


# Procedure: xfstripchart_getelemopt
proc xfstripchart_getelemopt { stripchart_path element} {
    global xfstripchartelemOptions xfstripchartelemDefaults xfstripchartcurelem

    set xfstripchartcurelem $element
    foreach item [$stripchart_path element configure $element] {
	if {[llength $item] < 4} {
	    continue
	}
	set name [lindex $item 0] 
	set value [lindex $item 4]
	set xfstripchartelemOptions($name) $value
    }
    set names [lsort [array names xfstripchartelemOptions]]
    set xfstripchartelemDefaults {}
    foreach i $names {
	lappend xfstripchartelemDefaults $i $xfstripchartelemOptions($i)
    }
}


# Procedure: xfstripchart_getlegendopt
proc xfstripchart_getlegendopt { stripchart_path} {
    global xfstripchartlegendOptions xfstripchartlegendDefaults

    foreach item [$stripchart_path legend configure] {
	if {[llength $item] < 4} {
	    continue
	}
	set name [lindex $item 0] 
	set value [lindex $item 4]
	set xfstripchartlegendOptions($name) $value
    }
    set names [lsort [array names xfstripchartlegendOptions]]
    set xfstripchartlegendDefaults {}
    foreach i $names {
	lappend xfstripchartlegendDefaults $i $xfstripchartlegendOptions($i)
    }
}


# Procedure: xfstripchart_getopt
proc xfstripchart_getopt { stripchart_path} {
    global xfstripchart xfstripchartDefaults

    foreach item [$stripchart_path configure] {
	if {[llength $item] < 4} {
	    continue
	}
	set name [lindex $item 0] 
	set value [lindex $item 4]
	set xfstripchart($name) $value
    }
    set names [lsort [array names xfstripchart]]
    set xfstripchartDefaults {}
    foreach i $names {
	lappend xfstripchartDefaults $i $xfstripchart($i)
    }
}


# Procedure: xfstripchart_getpsopt
proc xfstripchart_getpsopt { stripchart_path} {
    global xfstripchartpsOptions xfstripchartpsDefaults

    foreach item [$stripchart_path postscript configure] {
	if {[llength $item] < 4} {
	    continue
	}
	set name [lindex $item 0] 
	set value [lindex $item 4]
	set xfstripchartpsOptions($name) $value
    }
    set names [lsort [array names xfstripchartpsOptions]]
    set xfstripchartpsDefaults {}
    foreach i $names {
	lappend xfstripchartpsDefaults $i $xfstripchartpsOptions($i)
    }
}


# Procedure: xfstripchart_getxhairopt
proc xfstripchart_getxhairopt { stripchart_path} {
    global xfstripchartxhairOptions xfstripchartxhairDefaults

    foreach item [$stripchart_path crosshairs configure] {
	if {[llength $item] < 4} {
	    continue
	}
	set name [lindex $item 0] 
	set value [lindex $item 4]
	set xfstripchartxhairOptions($name) $value
    }
    set names [lsort [array names xfstripchartxhairOptions]]
    set xfstripchartxhairDefaults {}
    foreach i $names {
	lappend xfstripchartxhairDefaults $i $xfstripchartxhairOptions($i)
    }
}


# Procedure: xfstripchartcfg_axis
proc xfstripchartcfg_axis { stripchart_path} {
    global xfstripchartaxisOptions xfstripchartaxisDefaults 
    global xfstripchart_parent xfstripchart_name xfstripchartcuraxis xfstripchart_path

    set xfstripchart_parent [winfo parent $stripchart_path]
    set xfstripchart_name [winfo name $stripchart_path]
    set xfstripchart_path $stripchart_path

    xfstripchart_getaxisopt $stripchart_path xaxis
    xfstripchartshow.xfstripchartaxis
    set xfstripchartcuraxis xaxis
}


# Procedure: xfstripchartcfg_elem
proc xfstripchartcfg_elem { stripchart_path} {
    global xfstripchartelemOptions xfstripchartelemDefaults
    global xfstripchart_parent xfstripchart_name xfstripchartcurelem xfstripchart_path

    set xfstripchart_parent [winfo parent $stripchart_path]
    set xfstripchart_name [winfo name $stripchart_path]
    set xfstripchart_path $stripchart_path

    set elements [lsort [$stripchart_path element names]]
    if	{ [llength $elements] > 0 } {
	  set element [lindex $elements 0]
	  xfstripchart_getelemopt $stripchart_path $element
	} {
	  set xfstripchartcurelem ""
	}

    xfstripchartshow.xfstripchartelem
    update

    set count 0
    if {[winfo exists .xfstripchartelem.frame7.elemnames.frame]} {destroy .xfstripchartelem.frame7.elemnames.frame}
    frame .xfstripchartelem.frame7.elemnames.frame
    pack .xfstripchartelem.frame7.elemnames.frame
    foreach i $elements {
	radiobutton .xfstripchartelem.frame7.elemnames.frame.rbutton_$i -variable xfstripchartcurelem -value $i -text $i  -command "xfstripchart_getelemopt $stripchart_path $i"
	pack .xfstripchartelem.frame7.elemnames.frame.rbutton_$i -side left
	incr count
    }
}


# Procedure: xfstripchartcfg_gen
proc xfstripchartcfg_gen { stripchart_path} {
    global xfstripchart xfstripchartDefaults
    global xfstripchart_parent xfstripchart_name xfstripchart_path

    set xfstripchart_parent [winfo parent $stripchart_path]
    set xfstripchart_name [winfo name $stripchart_path]
    set xfstripchart_path $stripchart_path

    xfstripchart_getopt $stripchart_path
    xfstripchartshow.xfstripchartgen
}


# Procedure: xfstripchartcfg_legend
proc xfstripchartcfg_legend { stripchart_path} {
    global xfstripchartlegendOptions xfstripchartlegendDefaults
    global xfstripchart_parent xfstripchart_name xfstripchart_path

    set xfstripchart_parent [winfo parent $stripchart_path]
    set xfstripchart_name [winfo name $stripchart_path]
    set xfstripchart_path $stripchart_path

    xfstripchart_getlegendopt $stripchart_path
    xfstripchartshow.xfstripchartlegend
}


# Procedure: xfstripchartcfg_ps
proc xfstripchartcfg_ps { stripchart_path} {
    global xfstripchartpsOptions xfstripchartpsDefaults
    global xfstripchart_parent xfstripchart_name xfstripchart_path

    set xfstripchart_parent [winfo parent $stripchart_path]
    set xfstripchart_name [winfo name $stripchart_path]
    set xfstripchart_path $stripchart_path

    xfstripchart_getpsopt $stripchart_path
    xfstripchartshow.xfstripchartps
}


# Procedure: xfstripchartcfg_xhair
proc xfstripchartcfg_xhair { stripchart_path} {
    global xfstripchartxhairOptions xfstripchartxhairDefaults
    global xfstripchart_parent xfstripchart_name xfstripchart_path

    set xfstripchart_parent [winfo parent $stripchart_path]
    set xfstripchart_name [winfo name $stripchart_path]
    set xfstripchart_path $stripchart_path

    xfstripchart_getxhairopt $stripchart_path
    xfstripchartshow.xfstripchartxhair
}

