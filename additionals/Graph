  pack configure .xfgraphps.frame7.frame40.radiobutton42  -side left

  # pack master .xfgraphps.frame7.frame18
  pack configure .xfgraphps.frame7.frame18.label18  -side left
  pack configure .xfgraphps.frame7.frame18.radiobutton41  -side left
  pack configure .xfgraphps.frame7.frame18.radiobutton42  -side left

  # pack master .xfgraphps.frame7.frame20
  pack configure .xfgraphps.frame7.frame20.label18  -side left
  pack configure .xfgraphps.frame7.frame20.entry19  -expand 1  -fill x  -side left

  # pack master .xfgraphps.frame7.frame23
  pack configure .xfgraphps.frame7.frame23.label18  -side left
  pack configure .xfgraphps.frame7.frame23.entry19  -expand 1  -fill x  -side left

  # pack master .xfgraphps.frame7.frame25
  pack configure .xfgraphps.frame7.frame25.label18  -side left
  pack configure .xfgraphps.frame7.frame25.entry19  -expand 1  -fill x  -side left

  # pack master .xfgraphps.frame7.frame30
  pack configure .xfgraphps.frame7.frame30.label18  -side left
  pack configure .xfgraphps.frame7.frame30.entry19  -expand 1  -fill x  -side left

  # pack master .xfgraphps.frame7.frame31
  pack configure .xfgraphps.frame7.frame31.label18  -side left
  pack configure .xfgraphps.frame7.frame31.entry19  -expand 1  -fill x  -side left

  # pack master .xfgraphps.frame8
  pack configure .xfgraphps.frame8.button13  -expand 1  -fill x  -side left
  pack configure .xfgraphps.frame8.button9  -expand 1  -fill x  -side left
  pack configure .xfgraphps.frame8.button10  -expand 1  -fill x  -side left
  pack configure .xfgraphps.frame8.button11  -expand 1  -fill x  -side left
  pack configure .xfgraphps.frame8.button12  -expand 1  -fill x  -side left

  # pack master .xfgraphps.frame15
