# procedure to show window xfgraphshow.xfgraphlegend
proc xfgraphshow.xfgraphlegend { args} {
  global xfBind

  # build widget .xfgraphlegend
  catch "destroy .xfgraphlegend"
  toplevel .xfgraphlegend   -relief {raised}

  # Window manager configurations
  wm positionfrom .xfgraphlegend ""
  wm sizefrom .xfgraphlegend ""
  wm maxsize .xfgraphlegend 1152 849
  wm minsize .xfgraphlegend 104 1
  wm title .xfgraphlegend {Graph Legend Configuration Options}


  # build widget .xfgraphlegend.frame7
  frame .xfgraphlegend.frame7  -borderwidth {2}  -height {30}  -relief {raised}  -width {30}

  # build widget .xfgraphlegend.frame7.frame17
  frame .xfgraphlegend.frame7.frame17  -height {30}  -width {30}

  # build widget .xfgraphlegend.frame7.frame17.label18
  label .xfgraphlegend.frame7.frame17.label18  -borderwidth {0}  -text {Activebackground:}  -width {16} -anchor e

  # build widget .xfgraphlegend.frame7.frame17.entry19
  entry .xfgraphlegend.frame7.frame17.entry19  -textvariable {xfgraphlegendOptions(-activebackground)}
  # bindings
  bind .xfgraphlegend.frame7.frame17.entry19 $xfBind(configure) {set xfgraphnewval [tk_chooseColor -parent .xfgraphlegend]
if {$xfgraphnewval != ""} {set xfgraphlegendOptions(-activebackground) $xfgraphnewval}}

  # build widget .xfgraphlegend.frame7.frame22
  frame .xfgraphlegend.frame7.frame22  -height {30}  -width {30}

  # build widget .xfgraphlegend.frame7.frame22.label21
  label .xfgraphlegend.frame7.frame22.label21  -borderwidth {0}  -text {Name:}  -width {16} -anchor e

  # build widget .xfgraphlegend.frame7.frame22.label25
  label .xfgraphlegend.frame7.frame22.label25  -borderwidth {0}  -text {graph0}  -textvariable {xfgraph_name}

  # build widget .xfgraphlegend.frame7.frame26
  frame .xfgraphlegend.frame7.frame26  -height {30}  -width {30}

  # build widget .xfgraphlegend.frame7.frame26.label18
  label .xfgraphlegend.frame7.frame26.label18  -borderwidth {0}  -text {Activeborderwidth:}  -width {16}

  # build widget .xfgraphlegend.frame7.frame26.entry19
  entry .xfgraphlegend.frame7.frame26.entry19  -textvariable {xfgraphlegendOptions(-activeborderwidth)}

  # build widget .xfgraphlegend.frame7.frame27
  frame .xfgraphlegend.frame7.frame27  -height {30}  -width {30}

  # build widget .xfgraphlegend.frame7.frame27.label18
  label .xfgraphlegend.frame7.frame27.label18  -borderwidth {0}  -text {Activeforeground:}  -width {16} -anchor e
  # bindings
  bind .xfgraphlegend.frame7.frame27.label18 $xfBind(configure) {set xfgraphnewval [tk_chooseColor -parent .xfgraphlegend]
if {$xfgraphnewval != ""} {set xfgraphlegendOptions(-activeforeground) $xfgraphnewval}}

  # build widget .xfgraphlegend.frame7.frame27.entry19
  entry .xfgraphlegend.frame7.frame27.entry19  -textvariable {xfgraphlegendOptions(-activeforeground)}
  # bindings
  bind .xfgraphlegend.frame7.frame27.entry19 $xfBind(configure) {set xfgraphnewval [tk_chooseColor -parent .xfgraphlegend]
if {$xfgraphnewval != ""} {set xfgraphlegendOptions(-activeforeground) $xfgraphnewval}}

  # build widget .xfgraphlegend.frame7.frame28
  frame .xfgraphlegend.frame7.frame28  -height {30}  -width {30}

  # build widget .xfgraphlegend.frame7.frame28.label18
  label .xfgraphlegend.frame7.frame28.label18  -borderwidth {0}  -text {Background:}  -width {16} -anchor e

  # build widget .xfgraphlegend.frame7.frame28.entry19
  entry .xfgraphlegend.frame7.frame28.entry19  -textvariable {xfgraphlegendOptions(-background)}
  # bindings
  bind .xfgraphlegend.frame7.frame28.entry19 $xfBind(configure) {set xfgraphnewval [tk_chooseColor -parent .xfgraphlegend]
if {$xfgraphnewval != ""} {set xfgraphlegendOptions(-background) $xfgraphnewval}}

  # build widget .xfgraphlegend.frame7.frame29
  frame .xfgraphlegend.frame7.frame29  -height {30}  -width {30}

  # build widget .xfgraphlegend.frame7.frame29.label18
  label .xfgraphlegend.frame7.frame29.label18  -borderwidth {0}  -text {Borderwidth:}  -width {16} -anchor e

  # build widget .xfgraphlegend.frame7.frame29.entry19
  entry .xfgraphlegend.frame7.frame29.entry19  -textvariable {xfgraphelemOptions(-outlinewidth)}
  # bindings
  bind .xfgraphlegend.frame7.frame29.entry19 $xfBind(configure) {set xfgraph(-font) [XFFontBox $xfFile(fonts)]}

  # build widget .xfgraphlegend.frame7.frame30
  frame .xfgraphlegend.frame7.frame30  -height {30}  -width {30}

  # build widget .xfgraphlegend.frame7.frame30.label18
  label .xfgraphlegend.frame7.frame30.label18  -borderwidth {0}  -text {Font:}  -width {16} -anchor e
  # bindings
  bind .xfgraphlegend.frame7.frame30.label18 $xfBind(configure) {set xfgraphnewval [tk_chooseColor -parent .xfgraphlegend]
if {$xfgraphnewval != ""} {set xfgraphlegendOptions(-font) $xfgraphnewval}}

