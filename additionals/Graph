  # build widget .xfgraphaxis.frame8.button13
  button .xfgraphaxis.frame8.button13  -padx {11}  -pady {4}  -text {Postscript}

  # build widget .xfgraphaxis.frame15
  frame .xfgraphaxis.frame15  -height {30}  -width {30}

  # build widget .xfgraphaxis.frame15.button9
  button .xfgraphaxis.frame15.button9  -command {set xfnewval "$xfgraph_path $xfgraphcuraxis configure"
foreach xfitem [array names xfgraphaxisOptions] {
	lappend xfnewval $xfitem
	lappend xfnewval [string trim $xfgraphaxisOptions($xfitem)]
      }
eval $xfnewval
xfgraphdestroy.xfgraphaxis}  -padx {11}  -pady {4}  -text {OK}

  # build widget .xfgraphaxis.frame15.button10
  button .xfgraphaxis.frame15.button10  -command {set xfnewval "$xfgraph_path $xfgraphcuraxis configure"
foreach xfitem [array names xfgraphaxisOptions] {
	lappend xfnewval $xfitem
	lappend xfnewval [string trim $xfgraphaxisOptions($xfitem)]
      }
eval $xfnewval}  -padx {11}  -pady {4}  -text {Apply}

  # build widget .xfgraphaxis.frame15.button12
  button .xfgraphaxis.frame15.button12  -command {eval "$xfgraph_path $xfgraphcuraxis configure $xfgraphaxisDefaults"
xfgraphdestroy.xfgraphaxis}  -padx {11}  -pady {4}  -text {Cancel}

  # build widget .xfgraphaxis.frame15.checkbutton16
  checkbutton .xfgraphaxis.frame15.checkbutton16  -text {Apply permanently}  -variable {xfConf(applyParameters)}

  # build widget .xfgraphaxis.frame22
  frame .xfgraphaxis.frame22  -borderwidth {2}  -height {30}  -relief {raised}  -width {30}

  # build widget .xfgraphaxis.frame22.label21
