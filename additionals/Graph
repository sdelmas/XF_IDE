  radiobutton .xfgraphlegend.frame7.frame13.radiobutton26  -highlightthickness {0}  -pady {0}  -text {groove }  -value {groove}  -variable {xfgraphlegendOptions(-relief)}  -width {7}

  # build widget .xfgraphlegend.frame8
  frame .xfgraphlegend.frame8  -height {30}  -width {30}

  # build widget .xfgraphlegend.frame8.button9
  button .xfgraphlegend.frame8.button9  -command {xfgraphcfg_gen $xfgraph_path}  -padx {11}  -pady {4}  -text {Parameters}

