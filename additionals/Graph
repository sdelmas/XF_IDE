  # build widget .xfgraphgen.frame15
  frame .xfgraphgen.frame15  -height {30}  -width {30}

  # build widget .xfgraphgen.frame15.button9
  button .xfgraphgen.frame15.button9  -command {set xfgraphDefaults ""
foreach xfitem [array names xfgraph] {
	lappend xfgraphDefaults $xfitem
	lappend xfgraphDefaults [string trim $xfgraph($xfitem)]
      }
eval "$xfgraph_path configure $xfgraphDefaults"
xfgraphdestroy.xfgraphgen}  -padx {11}  -pady {4}  -text {OK}

  # build widget .xfgraphgen.frame15.button10
  button .xfgraphgen.frame15.button10  -command {set xfnewval "$xfgraph_path configure"
foreach xfitem [array names xfgraph] {
	lappend xfnewval $xfitem
	lappend xfnewval [string trim $xfgraph($xfitem)]
      }
eval $xfnewval}  -padx {11}  -pady {4}  -text {Apply}

  # build widget .xfgraphgen.frame15.button12
  button .xfgraphgen.frame15.button12  -command {eval "$xfgraph_path configure $xfgraphDefaults"
xfgraphdestroy.xfgraphgen}  -padx {11}  -pady {4}  -text {Cancel}

  # build widget .xfgraphgen.frame15.checkbutton16
  checkbutton .xfgraphgen.frame15.checkbutton16  -text {Apply permanently}  -variable {xfConf(applyParameters)}

  # build widget .xfgraphgen.frame22
  frame .xfgraphgen.frame22  -borderwidth {2}  -height {30}  -relief {raised}  -width {30}

  # build widget .xfgraphgen.frame22.label21
  label .xfgraphgen.frame22.label21  -borderwidth {0}  -text {Widget path:}

  # build widget .xfgraphgen.frame22.label25
  label .xfgraphgen.frame22.label25  -borderwidth {0}  -text {.frame0}  -textvariable {xfgraph_parent}

  # pack master .xfgraphgen.frame7
  pack configure .xfgraphgen.frame7.frame1  -fill x
  pack configure .xfgraphgen.frame7.frame17  -fill x
  pack configure .xfgraphgen.frame7.frame26  -fill x
  pack configure .xfgraphgen.frame7.frame27  -fill x
