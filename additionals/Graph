  # build widget .xfgraphlegend.frame15
  frame .xfgraphlegend.frame15  -height {30}  -width {30}

  # build widget .xfgraphlegend.frame15.button9
  button .xfgraphlegend.frame15.button9  -command {set xfnewval "$xfgraph_path legend configure"
foreach xfitem [array names xfgraphlegendOptions] {
	lappend xfnewval $xfitem
	lappend xfnewval [string trim $xfgraphlegendOptions($xfitem)]
      }
eval $xfnewval
xfgraphdestroy.xfgraphlegend}  -padx {11}  -pady {4}  -text {OK}

  # build widget .xfgraphlegend.frame15.button10
  button .xfgraphlegend.frame15.button10  -command {set xfnewval "$xfgraph_path legend configure"
foreach xfitem [array names xfgraphlegendOptions] {
	lappend xfnewval $xfitem
	lappend xfnewval [string trim $xfgraphlegendOptions($xfitem)]
      }
eval $xfnewval}  -padx {11}  -pady {4}  -text {Apply}

  # build widget .xfgraphlegend.frame15.button12
  button .xfgraphlegend.frame15.button12  -command {eval "$xfgraph_path legend configure $xfgraphlegendDefaults"
xfgraphdestroy.xfgraphlegend}  -padx {11}  -pady {4}  -text {Cancel}

  # build widget .xfgraphlegend.frame15.checkbutton16
  checkbutton .xfgraphlegend.frame15.checkbutton16  -text {Apply permanently}  -variable {xfConf(applyParameters)}

  # build widget .xfgraphlegend.frame22
