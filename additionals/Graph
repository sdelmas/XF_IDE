  # build widget .xfgraphaxis.frame22.label25
  label .xfgraphaxis.frame22.label25  -borderwidth {0}  -text {.frame0}  -textvariable {xfgraph_parent}

  # pack master .xfgraphaxis.frame7
  pack configure .xfgraphaxis.frame7.frame22  -fill x
