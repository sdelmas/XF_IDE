  entry .xfgraphaxis.frame7.frame29.entry19  -textvariable {xfgraphaxisOptions(-min)}

  # build widget .xfgraphaxis.frame7.frame30
  frame .xfgraphaxis.frame7.frame30  -height {30}  -width {30}

  # build widget .xfgraphaxis.frame7.frame30.label18
