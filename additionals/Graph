  pack configure .xfgraphaxis.frame7.elemnames.xaxis3  -side left
  pack configure .xfgraphaxis.frame7.elemnames.xaxis4  -side left
  pack configure .xfgraphaxis.frame7.elemnames.xaxis5  -side left

  # pack master .xfgraphaxis.frame7.frame6
  pack configure .xfgraphaxis.frame7.frame6.label18  -side left
  pack configure .xfgraphaxis.frame7.frame6.radiobutton41  -side left
  pack configure .xfgraphaxis.frame7.frame6.radiobutton42  -side left

  # pack master .xfgraphaxis.frame7.frame8
  pack configure .xfgraphaxis.frame7.frame8.label18  -side left
  pack configure .xfgraphaxis.frame7.frame8.radiobutton41  -side left
  pack configure .xfgraphaxis.frame7.frame8.radiobutton42  -side left

  # pack master .xfgraphaxis.frame7.frame10
  pack configure .xfgraphaxis.frame7.frame10.label18  -side left
  pack configure .xfgraphaxis.frame7.frame10.radiobutton41  -side left
  pack configure .xfgraphaxis.frame7.frame10.radiobutton42  -side left

  # pack master .xfgraphaxis.frame7.frame12
  pack configure .xfgraphaxis.frame7.frame12.label18  -side left
  pack configure .xfgraphaxis.frame7.frame12.radiobutton41  -side left
  pack configure .xfgraphaxis.frame7.frame12.radiobutton42  -side left

  # pack master .xfgraphaxis.frame7.frame15
  pack configure .xfgraphaxis.frame7.frame15.label18  -side left
  pack configure .xfgraphaxis.frame7.frame15.radiobutton41  -side left
  pack configure .xfgraphaxis.frame7.frame15.radiobutton42  -side left
  pack configure .xfgraphaxis.frame7.frame15.radiobutton17  -side left

  # pack master .xfgraphaxis.frame8
  pack configure .xfgraphaxis.frame8.button9  -expand 1  -fill x  -side left
  pack configure .xfgraphaxis.frame8.button10  -expand 1  -fill x  -side left
  pack configure .xfgraphaxis.frame8.button11  -expand 1  -fill x  -side left
  pack configure .xfgraphaxis.frame8.button12  -expand 1  -fill x  -side left
  pack configure .xfgraphaxis.frame8.button13  -expand 1  -fill x  -side left

  # pack master .xfgraphaxis.frame15
