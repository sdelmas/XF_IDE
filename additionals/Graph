  # build widget .xfgraphaxis.frame7.frame32
  frame .xfgraphaxis.frame7.frame32  -height {30}  -width {30}

  # build widget .xfgraphaxis.frame7.frame32.label18
  label .xfgraphaxis.frame7.frame32.label18  -borderwidth {0}  -text {Color:}  -width {16} -anchor e

  # build widget .xfgraphaxis.frame7.frame32.entry19
  entry .xfgraphaxis.frame7.frame32.entry19  -textvariable {xfgraphaxisOptions(-color)}
  # bindings
  bind .xfgraphaxis.frame7.frame32.entry19 $xfBind(configure) {set xfgraphnewval [tk_chooseColor -parent .xfgraphaxis]
if {$xfgraphnewval != ""} {set xfgraphaxisOptions(-color) $xfgraphnewval}}

