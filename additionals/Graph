  # build widget .xfgraphelem.frame8
  frame .xfgraphelem.frame8  -height {30}  -width {30}

  # build widget .xfgraphelem.frame8.button9
  button .xfgraphelem.frame8.button9  -command {xfgraphcfg_gen $xfgraph_path}  -padx {11}  -pady {4}  -text {Parameters}

  # build widget .xfgraphelem.frame8.button10
  button .xfgraphelem.frame8.button10  -command {xfgraphcfg_axis $xfgraph_path}  -padx {11}  -pady {4}  -text {Axis}

  # build widget .xfgraphelem.frame8.button11
  button .xfgraphelem.frame8.button11  -padx {11}  -pady {4}  -text {Legend}

  # build widget .xfgraphelem.frame8.button12
  button .xfgraphelem.frame8.button12  -padx {11}  -pady {4}  -text {Crosshairs}

  # build widget .xfgraphelem.frame8.button13
  button .xfgraphelem.frame8.button13  -padx {11}  -pady {4}  -text {Postscript}

  # build widget .xfgraphelem.frame15
  frame .xfgraphelem.frame15  -height {30}  -width {30}

  # build widget .xfgraphelem.frame15.button9
  button .xfgraphelem.frame15.button9  -command {set xfnewval "$xfgraph_path element configure $xfgraphcurelem"
foreach xfitem [lsort [array names xfgraphelemOptions]] {
	lappend xfnewval $xfitem
	lappend xfnewval [string trim $xfgraphelemOptions($xfitem)]
      }
eval $xfnewval
xfgraphdestroy.xfgraphelem}  -padx {11}  -pady {4}  -text {OK}

  # build widget .xfgraphelem.frame15.button10
  button .xfgraphelem.frame15.button10  -command {if {$xfgraphcurelem != ""} {
	set xfnewval "$xfgraph_path element configure $xfgraphcurelem"
	foreach xfitem [lsort [array names xfgraphelemOptions]] {
		lappend xfnewval $xfitem
		lappend xfnewval [string trim $xfgraphelemOptions($xfitem)]
	      }
	eval $xfnewval
   }}  -padx {11}  -pady {4}  -text {Apply}

  # build widget .xfgraphelem.frame15.button12
  button .xfgraphelem.frame15.button12  -command {if {$xfgraphcurelem != ""} {
	eval "$xfgraph_path element configure $xfgraphcurelem $xfgraphelemDefaults"
   }
xfgraphdestroy.xfgraphelem}  -padx {11}  -pady {4}  -text {Cancel}

  # build widget .xfgraphelem.frame15.checkbutton16
  checkbutton .xfgraphelem.frame15.checkbutton16  -text {Apply permanently}  -variable {xfConf(applyParameters)}

  # build widget .xfgraphelem.frame22
  frame .xfgraphelem.frame22  -borderwidth {2}  -height {30}  -relief {raised}  -width {30}

  # build widget .xfgraphelem.frame22.label21
  label .xfgraphelem.frame22.label21  -borderwidth {0}  -text {Widget path:}

  # build widget .xfgraphelem.frame22.label25
  label .xfgraphelem.frame22.label25  -borderwidth {0}  -text {.frame0}  -textvariable {xfgraph_parent}

  # build widget .xfgraphelem.frame0
  frame .xfgraphelem.frame0  -height {30}  -width {30}

  # build widget .xfgraphelem.frame0.button3
  button .xfgraphelem.frame0.button3  -command {set elemlist [$xfgraph_path element names]
set ctr [llength $elemlist]
incr ctr
while {[lsearch $elemlist $ctr] != -1} {
	incr ctr
      }
$xfgraph_path element create $ctr
xfgraphcfg_elem $xfgraph_path}  -padx {11}  -pady {4}  -text {Add Element}

  # build widget .xfgraphelem.frame0.button2
  button .xfgraphelem.frame0.button2  -command {if {$xfgraphcurelem != ""} {
    $xfgraph_path element delete $xfgraphcurelem
    xfgraphcfg_elem $xfgraph_path
   }}  -padx {11}  -pady {4}  -text {Delete Element}

  # pack master .xfgraphelem.frame7
  pack configure .xfgraphelem.frame7.frame22  -fill x
  pack configure .xfgraphelem.frame7.elemnames  -fill x
  pack configure .xfgraphelem.frame7.frame17  -fill x
  pack configure .xfgraphelem.frame7.frame26  -fill x
  pack configure .xfgraphelem.frame7.frame29  -fill x
  pack configure .xfgraphelem.frame7.frame30  -fill x
  pack configure .xfgraphelem.frame7.frame31  -fill x
  pack configure .xfgraphelem.frame7.frame43  -fill x
  pack configure .xfgraphelem.frame7.frame33  -fill x
  pack configure .xfgraphelem.frame7.frame34  -fill x
  pack configure .xfgraphelem.frame7.frame40  -fill x
  pack configure .xfgraphelem.frame7.frame14  -fill x
  pack configure .xfgraphelem.frame7.frame48  -fill x
  pack configure .xfgraphelem.frame7.frame41  -fill x
  pack configure .xfgraphelem.frame7.frame36  -fill x
  pack configure .xfgraphelem.frame7.frame21  -fill x
  pack configure .xfgraphelem.frame7.frame52  -fill x
  pack configure .xfgraphelem.frame7.frame37  -fill x
  pack configure .xfgraphelem.frame7.frame39  -fill x
  pack configure .xfgraphelem.frame7.frame38  -fill x

  # pack master .xfgraphelem.frame7.frame17
  pack configure .xfgraphelem.frame7.frame17.label18  -side left
  pack configure .xfgraphelem.frame7.frame17.entry19  -expand 1  -fill x  -side left

