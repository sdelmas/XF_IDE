  # build widget .xfgraphaxis.frame7.frame22.label25
  label .xfgraphaxis.frame7.frame22.label25  -borderwidth {0}  -text {graph0}  -textvariable {xfgraph_name}

  # build widget .xfgraphaxis.frame7.frame26
  frame .xfgraphaxis.frame7.frame26  -height {30}  -width {30}

  # build widget .xfgraphaxis.frame7.frame26.label18
  label .xfgraphaxis.frame7.frame26.label18  -borderwidth {0}  -text {Titlefont:}  -width {16} -anchor e

  # build widget .xfgraphaxis.frame7.frame26.entry19
  entry .xfgraphaxis.frame7.frame26.entry19  -textvariable {xfgraphaxisOptions(-titlefont)}

  # bindings
  bind .xfgraphaxis.frame7.frame26.entry19 $xfBind(configure) {set xfgraphnewval [XFFontBox $xfFile(fonts)]
if {$xfgraphnewval != ""} {set xfgraphaxisOptions(-titlefont) $xfgraphnewval}}

  # build widget .xfgraphaxis.frame7.frame27
  frame .xfgraphaxis.frame7.frame27  -height {30}  -width {30}

  # build widget .xfgraphaxis.frame7.frame27.label18
  label .xfgraphaxis.frame7.frame27.label18  -borderwidth {0}  -text {Linewidth:}  -width {16} -anchor e

  # build widget .xfgraphaxis.frame7.frame27.entry19
  entry .xfgraphaxis.frame7.frame27.entry19  -textvariable {xfgraphaxisOptions(-linewidth)}

  # build widget .xfgraphaxis.frame7.frame28
  frame .xfgraphaxis.frame7.frame28  -height {30}  -width {30}

