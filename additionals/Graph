proc xfgraphdestroy.xfgraphlegend {} {
     catch "destroy .xfgraphlegend"
}


# procedure to show window xfgraphshow.xfgraphps
proc xfgraphshow.xfgraphps { args} {
  global xfBind

  # build widget .xfgraphps
  catch "destroy .xfgraphps"
  toplevel .xfgraphps   -relief {raised}

  # Window manager configurations
  wm positionfrom .xfgraphps ""
  wm sizefrom .xfgraphps ""
  wm maxsize .xfgraphps 1152 849
  wm minsize .xfgraphps 104 10
  wm title .xfgraphps {Graph Postscript Configuration}


  # build widget .xfgraphps.frame7
  frame .xfgraphps.frame7  -borderwidth {2}  -height {30}  -relief {raised}  -width {30}

  # build widget .xfgraphps.frame7.frame17
  frame .xfgraphps.frame7.frame17  -height {30}  -width {30}

