    catch "destroy .xfgraphaxis"
}


# procedure to show window xfgraphshow.xfgraphconf
proc xfgraphshow.xfgraphconf { args} {
  global xfBind

  # build widget .xfgraphconf
  catch "destroy .xfgraphconf"
  toplevel .xfgraphconf 

  # Window manager configurations

  wm positionfrom .xfgraphconf ""
  wm sizefrom .xfgraphconf ""
  wm maxsize .xfgraphconf 1156 849
  wm minsize .xfgraphconf 104 1
  wm title .xfgraphconf {Graph Configure Tool}

  # build widget .xfgraphconf.cbutton_General
  checkbutton .xfgraphconf.cbutton_General  -command {ConfigureGeneral}  -text {General}  -variable {varGeneral}

  # build widget .xfgraphconf.cbutton_Element
  checkbutton .xfgraphconf.cbutton_Element  -command {ConfigureElement}  -text {Element}  -variable {varElement}

  # build widget .xfgraphconf.cbutton_Axis
  checkbutton .xfgraphconf.cbutton_Axis  -command {ConfigureAxis}  -text {Axis}  -variable {varAxis}

  # build widget .xfgraphconf.cbutton_Legend
  checkbutton .xfgraphconf.cbutton_Legend  -command {ConfigureLegend}  -text {Legend}  -variable {varLegend}

  # build widget .xfgraphconf.cbutton_Crosshairs
  checkbutton .xfgraphconf.cbutton_Crosshairs  -command {ConfigureCrosshairs}  -text {Crosshairs}  -variable {varCrosshairs}

  # build widget .xfgraphconf.cbutton_PostScript
  checkbutton .xfgraphconf.cbutton_PostScript  -command {ConfigurePostScript}  -text {PostScript}  -variable {varPostScript}

  # build widget .xfgraphconf.quit
  button .xfgraphconf.quit  -command {xfgraphdestroy.xfgraphconf}  -padx {11}  -pady {4}  -text {quit}

  # build widget .xfgraphconf.frame0
  frame .xfgraphconf.frame0  -borderwidth {2}  -height {30}  -relief {ridge}  -width {30}

  # build widget .xfgraphconf.frame0.label1
  label .xfgraphconf.frame0.label1  -background {steelblue}  -font {helvetica 12 bold}  -foreground {white}  -relief {groove}  -text {Path:}

  # build widget .xfgraphconf.frame0.entry2
  entry .xfgraphconf.frame0.entry2  -background {lightblue}  -font {helvetica 12 bold}

  # pack master .xfgraphconf.frame0
  pack configure .xfgraphconf.frame0.label1  -fill y  -side left
  pack configure .xfgraphconf.frame0.entry2  -expand 1  -fill x  -side left

  # pack master .xfgraphconf
  pack configure .xfgraphconf.frame0  -fill x
  pack configure .xfgraphconf.cbutton_General  -anchor w
  pack configure .xfgraphconf.cbutton_Element  -anchor w
  pack configure .xfgraphconf.cbutton_Axis  -anchor w
  pack configure .xfgraphconf.cbutton_Legend  -anchor w
  pack configure .xfgraphconf.cbutton_Crosshairs  -anchor w
  pack configure .xfgraphconf.cbutton_PostScript  -anchor w
  pack configure .xfgraphconf.quit

  .xfgraphconf.frame0.entry2 insert end {.frame0.xfgraph0}
}

proc xfgraphdestroy.xfgraphconf {} {# xf ignore me 7
    catch "destroy .xfgraphconf"
}


