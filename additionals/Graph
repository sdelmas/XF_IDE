     catch "destroy .xfgraphps"
}


# procedure to show window xfgraphshow.xfgraphxhair
proc xfgraphshow.xfgraphxhair { args} {
  global xfBind

  # build widget .xfgraphxhair
  catch "destroy .xfgraphxhair"
  toplevel .xfgraphxhair   -relief {raised}

  # Window manager configurations
  wm positionfrom .xfgraphxhair ""
  wm sizefrom .xfgraphxhair ""
  wm maxsize .xfgraphxhair 1152 849
  wm minsize .xfgraphxhair 104 10
  wm title .xfgraphxhair {Graph Crosshairs Configuration}

  # build widget .xfgraphxhair.frame7
  frame .xfgraphxhair.frame7  -borderwidth {2}  -height {30}  -relief {raised}  -width {30}

  # build widget .xfgraphxhair.frame7.frame17
  frame .xfgraphxhair.frame7.frame17  -height {30}  -width {30}

  # build widget .xfgraphxhair.frame7.frame17.label18
  label .xfgraphxhair.frame7.frame17.label18  -borderwidth {0}  -text {Color:}  -width {16} -anchor e

  # build widget .xfgraphxhair.frame7.frame17.entry19
  entry .xfgraphxhair.frame7.frame17.entry19  -textvariable {xfgraphxhairOptions(-color)}
  # bindings
  bind .xfgraphxhair.frame7.frame17.entry19 $xfBind(configure) {set xfgraphnewval [tk_chooseColor -parent .xfgraphxhair]
if {$xfgraphnewval != ""} {set xfgraphxhairOptions(-color) $xfgraphnewval}}

  # build widget .xfgraphxhair.frame7.frame22
  frame .xfgraphxhair.frame7.frame22  -height {30}  -width {30}

  # build widget .xfgraphxhair.frame7.frame22.label21
  label .xfgraphxhair.frame7.frame22.label21  -borderwidth {0}  -text {Name:}  -width {16} -anchor e

  # build widget .xfgraphxhair.frame7.frame22.label25
  label .xfgraphxhair.frame7.frame22.label25  -borderwidth {0}  -text {graph0}  -textvariable {xfgraph_name}

  # build widget .xfgraphxhair.frame7.frame26
  frame .xfgraphxhair.frame7.frame26  -height {30}  -width {30}

  # build widget .xfgraphxhair.frame7.frame26.label18
  label .xfgraphxhair.frame7.frame26.label18  -borderwidth {0}  -text {Dashes:}  -width {16} -anchor e

  # build widget .xfgraphxhair.frame7.frame26.entry19
  entry .xfgraphxhair.frame7.frame26.entry19  -textvariable {xfgraphxhairOptions(-dashes)}

  # build widget .xfgraphxhair.frame7.frame27
  frame .xfgraphxhair.frame7.frame27  -height {30}  -width {30}

  # build widget .xfgraphxhair.frame7.frame27.label18
  label .xfgraphxhair.frame7.frame27.label18  -borderwidth {0}  -text {Linewidth:}  -width {16} -anchor e

  # build widget .xfgraphxhair.frame7.frame27.entry19
  entry .xfgraphxhair.frame7.frame27.entry19  -textvariable {xfgraphxhairOptions(-linewidth)}

  # build widget .xfgraphxhair.frame7.frame28
  frame .xfgraphxhair.frame7.frame28  -height {30}  -width {30}

  # build widget .xfgraphxhair.frame7.frame28.label18
  label .xfgraphxhair.frame7.frame28.label18  -borderwidth {0}  -text {Position:}  -width {16} -anchor e

  # build widget .xfgraphxhair.frame7.frame28.entry19
  entry .xfgraphxhair.frame7.frame28.entry19  -textvariable {xfgraphxhairOptions(-position)}

  # build widget .xfgraphxhair.frame7.frame40
  frame .xfgraphxhair.frame7.frame40  -height {30}  -width {30}

  # build widget .xfgraphxhair.frame7.frame40.label18
  label .xfgraphxhair.frame7.frame40.label18  -borderwidth {0}  -text {Hide:}  -width {16}

  # build widget .xfgraphxhair.frame7.frame40.radiobutton41
  radiobutton .xfgraphxhair.frame7.frame40.radiobutton41  -text {true}  -value {1}  -variable {xfgraphxhairOptions(-hide)}

  # build widget .xfgraphxhair.frame7.frame40.radiobutton42
  radiobutton .xfgraphxhair.frame7.frame40.radiobutton42  -text {false}  -value {0}  -variable {xfgraphxhairOptions(-hide)}

  # build widget .xfgraphxhair.frame8
  frame .xfgraphxhair.frame8  -height {30}  -width {30}

  # build widget .xfgraphxhair.frame8.button9
  button .xfgraphxhair.frame8.button9  -command {xfgraphcfg_elem $xfgraph_path}  -padx {11}  -pady {4}  -text {Element}

  # build widget .xfgraphxhair.frame8.button10
  button .xfgraphxhair.frame8.button10  -command {xfgraphcfg_axis $xfgraph_path}  -padx {11}  -pady {4}  -text {Axis}

  # build widget .xfgraphxhair.frame8.button11
  button .xfgraphxhair.frame8.button11  -command {xfgraphcfg_legend $xfgraph_path}  -padx {11}  -pady {4}  -text {Legend}

  # build widget .xfgraphxhair.frame8.button12
  button .xfgraphxhair.frame8.button12  -command {xfgraphcfg_gen $xfgraph_path}  -padx {11}  -pady {4}  -text {Parameters}

  # build widget .xfgraphxhair.frame8.button13
  button .xfgraphxhair.frame8.button13  -command {xfgraphcfg_ps $xfgraph_path}  -padx {11}  -pady {4}  -text {Postscript}

  # build widget .xfgraphxhair.frame15
  frame .xfgraphxhair.frame15  -height {30}  -width {30}

  # build widget .xfgraphxhair.frame15.button9
  button .xfgraphxhair.frame15.button9  -command {set xfgraphDefaults ""
foreach xfitem [array names xfgraphxhairOptions] {
	lappend xfgraphxhairDefaults $xfitem
	lappend xfgraphxhairDefaults [string trim $xfgraphxhairOptions($xfitem)]
      }
eval "$xfgraph_path crosshairs configure $xfgraphxhairDefaults"
xfgraphdestroy.xfgraphxhair}  -padx {11}  -pady {4}  -text {OK}

  # build widget .xfgraphxhair.frame15.button10
  button .xfgraphxhair.frame15.button10  -command {set xfnewval "$xfgraph_path crosshairs configure"
foreach xfitem [array names xfgraphxhairOptions] {
	lappend xfnewval $xfitem
	lappend xfnewval [string trim $xfgraphxhairOptions($xfitem)]
      }
eval $xfnewval}  -padx {11}  -pady {4}  -text {Apply}

  # build widget .xfgraphxhair.frame15.button12
  button .xfgraphxhair.frame15.button12  -command {eval "$xfgraph_path crosshairs configure $xfgraphxhairDefaults"
xfgraphdestroy.xfgraphxhair}  -padx {11}  -pady {4}  -text {Cancel}

  # build widget .xfgraphxhair.frame15.checkbutton16
  checkbutton .xfgraphxhair.frame15.checkbutton16  -text {Apply permanently}  -variable {xfConf(applyParameters)}

  # build widget .xfgraphxhair.frame22
  frame .xfgraphxhair.frame22  -borderwidth {2}  -height {30}  -relief {raised}  -width {30}

  # build widget .xfgraphxhair.frame22.label21
  label .xfgraphxhair.frame22.label21  -borderwidth {0}  -text {Widget path:}

  # build widget .xfgraphxhair.frame22.label25
  label .xfgraphxhair.frame22.label25  -borderwidth {0}  -text {.frame0}  -textvariable {xfgraph_parent}

  # pack master .xfgraphxhair.frame7
  pack configure .xfgraphxhair.frame7.frame22  -fill x
  pack configure .xfgraphxhair.frame7.frame17  -fill x
  pack configure .xfgraphxhair.frame7.frame26  -fill x
  pack configure .xfgraphxhair.frame7.frame27  -fill x
  pack configure .xfgraphxhair.frame7.frame40  -fill x
  pack configure .xfgraphxhair.frame7.frame28  -fill x

  # pack master .xfgraphxhair.frame7.frame17
  pack configure .xfgraphxhair.frame7.frame17.label18  -side left
  pack configure .xfgraphxhair.frame7.frame17.entry19  -expand 1  -fill x  -side left

  # pack master .xfgraphxhair.frame7.frame22
  pack configure .xfgraphxhair.frame7.frame22.label21  -side left
  pack configure .xfgraphxhair.frame7.frame22.label25  -anchor w  -pady 4

  # pack master .xfgraphxhair.frame7.frame26
  pack configure .xfgraphxhair.frame7.frame26.label18  -side left
  pack configure .xfgraphxhair.frame7.frame26.entry19  -expand 1  -fill x  -side left

  # pack master .xfgraphxhair.frame7.frame27
  pack configure .xfgraphxhair.frame7.frame27.label18  -side left
  pack configure .xfgraphxhair.frame7.frame27.entry19  -expand 1  -fill x  -side left

  # pack master .xfgraphxhair.frame7.frame28
  pack configure .xfgraphxhair.frame7.frame28.label18  -side left
  pack configure .xfgraphxhair.frame7.frame28.entry19  -expand 1  -fill x  -side left

  # pack master .xfgraphxhair.frame7.frame40
  pack configure .xfgraphxhair.frame7.frame40.label18  -side left
  pack configure .xfgraphxhair.frame7.frame40.radiobutton41  -side left
  pack configure .xfgraphxhair.frame7.frame40.radiobutton42  -side left

  # pack master .xfgraphxhair.frame8
  pack configure .xfgraphxhair.frame8.button12  -expand 1  -fill x  -side left
  pack configure .xfgraphxhair.frame8.button9  -expand 1  -fill x  -side left
  pack configure .xfgraphxhair.frame8.button10  -expand 1  -fill x  -side left
  pack configure .xfgraphxhair.frame8.button11  -expand 1  -fill x  -side left
  pack configure .xfgraphxhair.frame8.button13  -expand 1  -fill x  -side left

  # pack master .xfgraphxhair.frame15
  pack configure .xfgraphxhair.frame15.button9  -expand 1  -fill x  -side left
  pack configure .xfgraphxhair.frame15.button10  -expand 1  -fill x  -side left
  pack configure .xfgraphxhair.frame15.checkbutton16  -expand 1  -fill both  -side left
  pack configure .xfgraphxhair.frame15.button12  -expand 1  -fill x  -side left

  # pack master .xfgraphxhair.frame22
