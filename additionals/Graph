# Procedure: xfgraph_getaxisopt
proc xfgraph_getaxisopt { graph_path axis} {
    global xfgraphaxisOptions xfgraphaxisDefaults

    foreach item [$graph_path $axis configure] {
	if {[llength $item] < 4} {
	    continue
	}
	set name [lindex $item 0] 
	set value [lindex $item 4]
	set xfgraphaxisOptions($name) $value
    }
    set names [lsort [array names xfgraphaxisOptions]]
    set xfgraphaxisDefaults {}
    foreach i $names {
	lappend xfgraphaxisDefaults $i $xfgraphaxisOptions($i)
    }
}


# Procedure: xfgraph_getelemopt
proc xfgraph_getelemopt { graph_path element} {
    global xfgraphelemOptions xfgraphelemDefaults xfgraphcurelem

    set xfgraphcurelem $element
    foreach item [$graph_path element configure $element] {
	if {[llength $item] < 4} {
	    continue
	}
	set name [lindex $item 0] 
	set value [lindex $item 4]
	set xfgraphelemOptions($name) $value
    }
    set names [lsort [array names xfgraphelemOptions]]
    set xfgraphelemDefaults {}
    foreach i $names {
	lappend xfgraphelemDefaults $i $xfgraphelemOptions($i)
    }
}


# Procedure: xfgraph_getlegendopt
proc xfgraph_getlegendopt { graph_path} {
    global xfgraphlegendOptions xfgraphlegendDefaults

    foreach item [$graph_path legend configure] {
	if {[llength $item] < 4} {
	    continue
	}
	set name [lindex $item 0] 
	set value [lindex $item 4]
	set xfgraphlegendOptions($name) $value
    }
    set names [lsort [array names xfgraphlegendOptions]]
    set xfgraphlegendDefaults {}
    foreach i $names {
	lappend xfgraphlegendDefaults $i $xfgraphlegendOptions($i)
    }
}


# Procedure: xfgraph_getopt
proc xfgraph_getopt { graph_path} {
    global xfgraph xfgraphDefaults

    foreach item [$graph_path configure] {
	if {[llength $item] < 4} {
	    continue
	}
	set name [lindex $item 0] 
	set value [lindex $item 4]
	set xfgraph($name) $value
    }
    set names [lsort [array names xfgraph]]
    set xfgraphDefaults {}
    foreach i $names {
	lappend xfgraphDefaults $i $xfgraph($i)
    }
}


# Procedure: xfgraph_getpsopt
proc xfgraph_getpsopt { graph_path} {
    global xfgraphpsOptions xfgraphpsDefaults

    foreach item [$graph_path postscript configure] {
	if {[llength $item] < 4} {
	    continue
	}
	set name [lindex $item 0] 
	set value [lindex $item 4]
	set xfgraphpsOptions($name) $value
    }
    set names [lsort [array names xfgraphpsOptions]]
    set xfgraphpsDefaults {}
    foreach i $names {
	lappend xfgraphpsDefaults $i $xfgraphpsOptions($i)
    }
}


# Procedure: xfgraph_getxhairopt
proc xfgraph_getxhairopt { graph_path} {
    global xfgraphxhairOptions xfgraphxhairDefaults

    foreach item [$graph_path crosshairs configure] {
	if {[llength $item] < 4} {
	    continue
	}
	set name [lindex $item 0] 
	set value [lindex $item 4]
	set xfgraphxhairOptions($name) $value
    }
    set names [lsort [array names xfgraphxhairOptions]]
    set xfgraphxhairDefaults {}
    foreach i $names {
	lappend xfgraphxhairDefaults $i $xfgraphxhairOptions($i)
    }
}


# Procedure: xfgraphcfg_axis
proc xfgraphcfg_axis { graph_path} {
    global xfgraphaxisOptions xfgraphaxisDefaults 
    global xfgraph_parent xfgraph_name xfgraphcuraxis xfgraph_path

    set xfgraph_parent [winfo parent $graph_path]
    set xfgraph_name [winfo name $graph_path]
    set xfgraph_path $graph_path

    xfgraph_getaxisopt $graph_path xaxis
    xfgraphshow.xfgraphaxis
    set xfgraphcuraxis xaxis
}


# Procedure: xfgraphcfg_elem
proc xfgraphcfg_elem { graph_path} {
    global xfgraphelemOptions xfgraphelemDefaults
    global xfgraph_parent xfgraph_name xfgraphcurelem xfgraph_path

    set xfgraph_parent [winfo parent $graph_path]
    set xfgraph_name [winfo name $graph_path]
    set xfgraph_path $graph_path

    set elements [lsort [$graph_path element names]]
    if	{ [llength $elements] > 0 } {
	  set element [lindex $elements 0]
	  xfgraph_getelemopt $graph_path $element
	} {
	  set xfgraphcurelem ""
	}

    xfgraphshow.xfgraphelem
    update

    set count 0
    if {[winfo exists .xfgraphelem.frame7.elemnames.frame]} {destroy .xfgraphelem.frame7.elemnames.frame}
    frame .xfgraphelem.frame7.elemnames.frame
    pack .xfgraphelem.frame7.elemnames.frame
    foreach i $elements {
	radiobutton .xfgraphelem.frame7.elemnames.frame.rbutton_$i -variable xfgraphcurelem -value $i -text $i  -command "xfgraph_getelemopt $graph_path $i"
	pack .xfgraphelem.frame7.elemnames.frame.rbutton_$i -side left
	incr count
    }
}


# Procedure: xfgraphcfg_gen
proc xfgraphcfg_gen { graph_path} {
    global xfgraph xfgraphDefaults
    global xfgraph_parent xfgraph_name xfgraph_path

    set xfgraph_parent [winfo parent $graph_path]
    set xfgraph_name [winfo name $graph_path]
    set xfgraph_path $graph_path

    xfgraph_getopt $graph_path
    xfgraphshow.xfgraphgen
}


# Procedure: xfgraphcfg_legend
proc xfgraphcfg_legend { graph_path} {
    global xfgraphlegendOptions xfgraphlegendDefaults
    global xfgraph_parent xfgraph_name xfgraph_path

    set xfgraph_parent [winfo parent $graph_path]
    set xfgraph_name [winfo name $graph_path]
    set xfgraph_path $graph_path

    xfgraph_getlegendopt $graph_path
    xfgraphshow.xfgraphlegend
}


# Procedure: xfgraphcfg_ps
proc xfgraphcfg_ps { graph_path} {
    global xfgraphpsOptions xfgraphpsDefaults
    global xfgraph_parent xfgraph_name xfgraph_path

    set xfgraph_parent [winfo parent $graph_path]
    set xfgraph_name [winfo name $graph_path]
    set xfgraph_path $graph_path

    xfgraph_getpsopt $graph_path
    xfgraphshow.xfgraphps
}


# Procedure: xfgraphcfg_xhair
proc xfgraphcfg_xhair { graph_path} {
    global xfgraphxhairOptions xfgraphxhairDefaults
    global xfgraph_parent xfgraph_name xfgraph_path

    set xfgraph_parent [winfo parent $graph_path]
    set xfgraph_name [winfo name $graph_path]
    set xfgraph_path $graph_path

    xfgraph_getxhairopt $graph_path
    xfgraphshow.xfgraphxhair
}

