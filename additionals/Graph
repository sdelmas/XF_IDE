  # bindings
  bind .xfgraphelem.frame7.frame17.entry19 $xfBind(configure) {set xfgraphnewval [tk_chooseColor -parent .xfgraphelem]
if {$xfgraphnewval != ""} {set xfgraphelemOptions(-fill) $xfgraphnewval}}

