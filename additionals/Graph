  pack configure .xfgraphaxis.frame22.label25  -anchor w  -expand 1  -side left

  # pack master .xfgraphaxis
  pack configure .xfgraphaxis.frame22  -fill x
  pack configure .xfgraphaxis.frame7  -expand 1  -fill both
  pack configure .xfgraphaxis.frame8  -fill x
  pack configure .xfgraphaxis.frame15  -fill x

}

