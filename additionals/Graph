     catch "destroy .xfgraphelem"
}


# procedure to show window xfgraphshow.xfgraphgen
proc xfgraphshow.xfgraphgen { args} {
  global xfBind

  # build widget .xfgraphgen
  catch "destroy .xfgraphgen"
  toplevel .xfgraphgen   -relief {raised}

  # Window manager configurations
  wm positionfrom .xfgraphgen ""
  wm sizefrom .xfgraphgen ""
  wm maxsize .xfgraphgen 1152 849
  wm minsize .xfgraphgen 104 10
  wm title .xfgraphgen {Graph Parameters}


  # build widget .xfgraphgen.frame7
  frame .xfgraphgen.frame7  -borderwidth {2}  -height {30}  -relief {raised}  -width {30}

  # build widget .xfgraphgen.frame7.frame17
  frame .xfgraphgen.frame7.frame17  -height {30}  -width {30}

  # build widget .xfgraphgen.frame7.frame17.label18
  label .xfgraphgen.frame7.frame17.label18  -borderwidth {0}  -text {Background:}  -width {16} -anchor e

  # build widget .xfgraphgen.frame7.frame17.entry19
  entry .xfgraphgen.frame7.frame17.entry19  -textvariable {xfgraph(-background)}
  # bindings
  bind .xfgraphgen.frame7.frame17.entry19 $xfBind(configure) {set xfgraphnewval [tk_chooseColor -parent .xfgraphgen]
if {$xfgraphnewval != ""} {set xfgraph(-background) $xfgraphnewval}}

  # build widget .xfgraphgen.frame7.frame26
  frame .xfgraphgen.frame7.frame26  -height {30}  -width {30}

  # build widget .xfgraphgen.frame7.frame26.label18
  label .xfgraphgen.frame7.frame26.label18  -borderwidth {0}  -text {Borderwidth:}  -width {16} -anchor e

  # build widget .xfgraphgen.frame7.frame26.entry19
  entry .xfgraphgen.frame7.frame26.entry19  -textvariable {xfgraph(-borderwidth)}

  # build widget .xfgraphgen.frame7.frame27
  frame .xfgraphgen.frame7.frame27  -height {30}  -width {30}

  # build widget .xfgraphgen.frame7.frame27.label18
  label .xfgraphgen.frame7.frame27.label18  -borderwidth {0}  -text {Bottommargin:}  -width {16} -anchor e

  # build widget .xfgraphgen.frame7.frame27.entry19
  entry .xfgraphgen.frame7.frame27.entry19  -textvariable {xfgraph(-bottommargin)}

  # build widget .xfgraphgen.frame7.frame28
  frame .xfgraphgen.frame7.frame28  -height {30}  -width {30}

  # build widget .xfgraphgen.frame7.frame28.label18
  label .xfgraphgen.frame7.frame28.label18  -borderwidth {0}  -text {Cursor:}  -width {16} -anchor e

  # build widget .xfgraphgen.frame7.frame28.entry19
  entry .xfgraphgen.frame7.frame28.entry19  -textvariable {xfgraph(-cursor)}
  # bindings
  bind .xfgraphgen.frame7.frame28.entry19 $xfBind(configure) {set xfnewval [CursorBox $xfFile(cursors)]
if {$xfnewval != ""} {set xfgraph(-cursor) $xfnewval}}

  # build widget .xfgraphgen.frame7.frame29
  frame .xfgraphgen.frame7.frame29  -height {30}  -width {30}

  # build widget .xfgraphgen.frame7.frame29.label18
  label .xfgraphgen.frame7.frame29.label18  -borderwidth {0}  -text {Font:}  -width {16} -anchor e

  # build widget .xfgraphgen.frame7.frame29.entry19
  entry .xfgraphgen.frame7.frame29.entry19  -textvariable {xfgraph(-font)}
  # bindings
  bind .xfgraphgen.frame7.frame29.entry19 $xfBind(configure) {set xfgraph(-font) [XFFontBox $xfFile(fonts)]}

  # build widget .xfgraphgen.frame7.frame30
  frame .xfgraphgen.frame7.frame30  -height {30}  -width {30}

  # build widget .xfgraphgen.frame7.frame30.label18
  label .xfgraphgen.frame7.frame30.label18  -borderwidth {0}  -text {Foreground:} -width {16} -anchor e

  # build widget .xfgraphgen.frame7.frame30.entry19
  entry .xfgraphgen.frame7.frame30.entry19  -textvariable {xfgraph(-foreground)}
  # bindings
  bind .xfgraphgen.frame7.frame30.entry19 $xfBind(configure) {set xfgraphnewval [tk_chooseColor -parent .xfgraphgen]
if {$xfgraphnewval != ""} {set xfgraph(-foreground) $xfgraphnewval}}

  # build widget .xfgraphgen.frame7.frame31
  frame .xfgraphgen.frame7.frame31  -height {30}  -width {30}

  # build widget .xfgraphgen.frame7.frame31.label18
  label .xfgraphgen.frame7.frame31.label18  -borderwidth {0}  -text {Halo:}  -width {16} -anchor e

  # build widget .xfgraphgen.frame7.frame31.entry19
  entry .xfgraphgen.frame7.frame31.entry19  -textvariable {xfgraph(-halo)}

  # build widget .xfgraphgen.frame7.frame32
  frame .xfgraphgen.frame7.frame32  -height {30}  -width {30}

  # build widget .xfgraphgen.frame7.frame32.label18
  label .xfgraphgen.frame7.frame32.label18  -borderwidth {0}  -text {Height:}  -width {16} -anchor e

  # build widget .xfgraphgen.frame7.frame32.entry19
  entry .xfgraphgen.frame7.frame32.entry19  -textvariable {xfgraph(-height)}

  # build widget .xfgraphgen.frame7.frame33
  frame .xfgraphgen.frame7.frame33  -height {30}  -width {30}

  # build widget .xfgraphgen.frame7.frame33.label18
  label .xfgraphgen.frame7.frame33.label18  -borderwidth {0}  -text {Leftmargin:}  -width {16} -anchor e

  # build widget .xfgraphgen.frame7.frame33.entry19
  entry .xfgraphgen.frame7.frame33.entry19  -textvariable {xfgraph(-leftmargin)}

  # build widget .xfgraphgen.frame7.frame34
  frame .xfgraphgen.frame7.frame34  -height {30}  -width {30}

