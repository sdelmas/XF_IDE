# Program: xf
# Description: handle BLT 2.4 graph widgets
#
# Created by Dennis LaBelle
#
set default [pwd]
set blt_version 2.4
catch {load blt24}
catch {namespace import blt::*} result

global xfDefaultConf
set xfDefaultConf(graph) 4

##########
# Procedure: XFAdd.Graph
# Description: add a Graph
# Arguments: xfW - the widget
#            xfName - a optional name
#            xfType - add or config
# Returns: none
# Sideeffects: none
##########
proc XFAdd.Graph {xfW xfName xfType} {
  global xfStatus

  XFEditSetStatus "Inserting Graph..."
  set xfName [XFMiscGetUniqueName $xfName graph]
  if {"$xfStatus(path)" == "."} {
    if {"$xfType" == "add"} {
      if {[catch "graph .$xfName" xfResult]} {
        XFProcError "$xfResult"
        XFEditSetStatus "Inserting Graph...aborted"
        return
      }
    } {
      if {[catch "Graph .$xfName" xfResult]} {
        XFProcError "$xfResult"
        XFEditSetStatus "Inserting Graph...aborted"
        return
      }
    }

    XFMiscPositionWidget .$xfName
    XFMiscBindWidgetTree .$xfName
  } {
    if {"$xfType" == "add"} {
      if {[catch "graph $xfStatus(path).$xfName" xfResult]} {
        XFProcError "$xfResult"
        XFEditSetStatus "Inserting Graph...aborted"
        return
      }
    } {
      if {[catch "graph $xfStatus(path).$xfName" xfResult]} {
        XFProcError "$xfResult"
        XFEditSetStatus "Inserting Graph...aborted"
        return
      }
    }

    XFMiscPositionWidget $xfStatus(path).$xfName
    XFMiscBindWidgetTree $xfStatus(path).$xfName
  }

  incr xfStatus(elementCounter)
  XFEditSetPath $xfStatus(path)
  XFEditSetStatus "Inserting Graph...done"
}

##########
# Procedure: XFConfig.Graph4
# Description: configure a Graph
# Arguments: xfW - the widget
#            xfType - config type (add or config)
#            xfClass - the class we configure
#            xfLeader - the leading window
# Returns: none
# Sideeffects: none
##########
proc XFConfig.Graph4 {xfW xfType xfClass {xfLeader ""}} {
  global xfStatus

  if {"$xfType" == "add"} {
    set xfName graph$xfStatus(elementCounter)
  } {
    set xfName [XFMiscPathName $xfW]
  }
  XFEditSetStatus "Calling parameter setting for Graph..."

  xfgraphcfg_gen $xfW
  if {[winfo exists .xfgraphelem]} {xfgraphcfg_elem $xfW}

  XFEditSetStatus "Calling parameter setting for Graph...done"
}

##########
# Procedure: XFGraphSetGraph4
# Description: set graph parameters
# Arguments: xfW - the widget
#            xfType - the type of setting (1 = set always, 0 = set
#                     only if permanent apply is on)
#            xfClass - the class we configure
#            xfParam1 - ignored parameter
# Returns: none
# Sideeffects: none
##########
proc XFGraphSetGraph4 {xfW xfType xfClass {xfParam1 ""}} {
  global xfConf
  global xfMisc

  if {$xfType == 0 && !$xfConf(applyParameters)} {
    return
  }
  if {"[info commands $xfW]" == ""} {
    return
  }
  XFMiscSetSymbolicName $xfW \
    [.xf${xfClass}Config4.params1.params2.symname.symname get]

  XFMiscSetResource $xfW background \
    [.xf${xfClass}Config4.params1.params2.bg.bg get]
}

##########
# Procedure: XFSaveSpecial.Graph
# Description: save bargraph
# Arguments: xfW - the widget
# Returns: none
# Sideeffects: none
##########
proc XFSaveSpecial.Graph {xfW} {
global xfgraphDefaults
global xfgraphcurelem
global xfgraphelemDefaults
global xfgraphaxisDefaults
global xfgraphlegendDefaults
global xfgraphxhairDefaults
global xfgraphpsDefaults
global xfgraphelemOptions

  xfgraph_getopt $xfW
  set rtnval "\n  $xfW configure $xfgraphDefaults"
  set vnamelist [vector names]
  set elements [lsort [$xfW element names]]
  foreach element $elements {
	    if {$element != ""} {
			xfgraph_getelemopt $xfW $element
			set vname $xfgraphelemOptions(-xdata)
			if {[lsearch $vnamelist $vname] != -1} {
				# Output code to support vector
				upvar #0 $vname vecname
				set rtnval "$rtnval\nglobal $vname"
				set rtnval "$rtnval\nif {\[info exists $vname\]} {unset $vname}"
				set rtnval "$rtnval\nvector create $vname"
				set rtnval "$rtnval\n  $vname set {$vecname(:)}"
			   }
			set vname $xfgraphelemOptions(-ydata)
			if {[lsearch $vnamelist $vname] != -1} {
				# Output code to support vector
				upvar #0 $vname vecname
				set rtnval "$rtnval\nglobal $vname"
				set rtnval "$rtnval\nif {\[info exists $vname\]} {unset $vname}"
				set rtnval "$rtnval\nvector create $vname"
				set rtnval "$rtnval\n  $vname set {$vecname(:)}"
			   }
			set rtnval "$rtnval\n  $xfW element create $element $xfgraphelemDefaults"
		 }
	  }
  xfgraph_getaxisopt $xfW xaxis
  set rtnval "$rtnval\n  $xfW xaxis configure $xfgraphaxisDefaults"
  xfgraph_getaxisopt $xfW yaxis
  set rtnval "$rtnval\n  $xfW yaxis configure $xfgraphaxisDefaults"
  xfgraph_getaxisopt $xfW x2axis
  set rtnval "$rtnval\n  $xfW x2axis configure $xfgraphaxisDefaults"
  xfgraph_getaxisopt $xfW y2axis
  set rtnval "$rtnval\n  $xfW y2axis configure $xfgraphaxisDefaults"
  xfgraph_getlegendopt $xfW
  set rtnval "$rtnval\n  $xfW legend configure $xfgraphlegendDefaults"
  xfgraph_getxhairopt $xfW
  set rtnval "$rtnval\n  $xfW crosshairs configure $xfgraphxhairDefaults"
  xfgraph_getpsopt $xfW
  set rtnval "$rtnval\n  $xfW postscript configure $xfgraphpsDefaults\n"
  return $rtnval
}
# procedure to show window xfgraphshow.xfgraphaxis
proc xfgraphshow.xfgraphaxis { args} {
  global xfBind

  # build widget .xfgraphaxis
  catch "destroy .xfgraphaxis"
  toplevel .xfgraphaxis   -relief {raised}

  # Window manager configurations
  wm positionfrom .xfgraphaxis ""
  wm sizefrom .xfgraphaxis ""
  wm maxsize .xfgraphaxis 1152 849
  wm minsize .xfgraphaxis 104 1
  wm title .xfgraphaxis {Graph Axis Configuration Options}


  # build widget .xfgraphaxis.frame7
  frame .xfgraphaxis.frame7  -borderwidth {2}  -height {30}  -relief {raised}  -width {30}

  # build widget .xfgraphaxis.frame7.frame22
  frame .xfgraphaxis.frame7.frame22  -height {30}  -width {30}

  # build widget .xfgraphaxis.frame7.frame22.label21
