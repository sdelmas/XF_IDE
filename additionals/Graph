  # build widget .xfgraphlegend.frame7.frame11.radiobutton26
  radiobutton .xfgraphlegend.frame7.frame11.radiobutton26  -highlightthickness {0}  -text {sunken}  -value {sunken}  -variable {xfgraphlegendOptions(-relief)}  -width {7}

  # build widget .xfgraphlegend.frame7.frame11.radiobutton27
